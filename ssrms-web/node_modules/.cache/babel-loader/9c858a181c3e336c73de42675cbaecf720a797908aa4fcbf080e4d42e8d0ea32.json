{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\nexport default {\n  name: 'LoginRegister',\n  data() {\n    return {\n      mode: 'login',\n      // 'login' 或 'register'\n      loginForm: {\n        account: '',\n        password: '',\n        role: 'student',\n        remember: true\n      },\n      registerForm: {\n        name: '',\n        account: '',\n        password: '',\n        confirmPassword: '',\n        role: 'student',\n        department: '',\n        agree: false\n      },\n      showAgreement: false\n    };\n  },\n  computed: {\n    isLogin() {\n      return this.mode === 'login';\n    }\n  },\n  methods: {\n    // 兼容 ElementUI / ElementPlus / 没装消息组件\n    toast(type, msg) {\n      try {\n        // ElementUI: this.$message\n        if (this.$message) return this.$message[type](msg);\n      } catch (e) {}\n      try {\n        // ElementPlus: ElMessage（如果你是按需引入，可自行 import）\n        if (window.ElMessage) return window.ElMessage[type](msg);\n      } catch (e) {}\n      alert(msg);\n    },\n    async handleLogin() {\n      if (!this.loginForm.account || !this.loginForm.password) {\n        this.toast('warning', '请填写账号和密码');\n        return;\n      }\n\n      // 你页面如果有“学生/管理员”选择，建议用字符串 role：student/admin\n      const roleStr = this.loginForm.role || 'student';\n      try {\n        // 如果你项目里已经把 axios 挂到 this.$axios，就用它；否则你就 import axios\n        const http = this.$axios;\n        const resp = await http.post('http://localhost:8090/auth/login', {\n          accountNo: this.loginForm.account,\n          password: this.loginForm.password,\n          role: roleStr\n        });\n        const r = resp.data;\n        if (!r || r.code !== 200) {\n          this.toast('error', r && r.message || '登录失败');\n          return;\n        }\n        const token = r.data && r.data.token;\n        const user = r.data && r.data.user ? r.data.user : {};\n\n        // 关键：保证 roleId 存在（0 管理员 / 1 学生）\n        const roleId = user.roleId !== undefined && user.roleId !== null ? user.roleId : user.role === 'admin' || roleStr === 'admin' ? 0 : 1;\n        const storeUser = {\n          ...user,\n          roleId\n        };\n        localStorage.setItem('ssrmsUser', JSON.stringify(storeUser));\n        if (token) localStorage.setItem('ssrmsToken', token);\n        this.toast('success', '登录成功');\n\n        // 支持守卫带回跳转\n        const redirect = this.$route?.query?.redirect;\n        if (redirect) this.$router.replace(redirect);else this.$router.replace(roleId === 0 ? '/admin' : '/user');\n      } catch (e) {\n        this.toast('error', e?.response?.data?.message || '登录请求失败，请检查后端是否启动/端口是否正确');\n      }\n    },\n    // 注册先不实现：保留 UI + 校验 + 提示\n    handleRegister() {\n      this.toast('info', '注册功能暂未接入后端接口，后续再实现');\n    }\n  }\n};","map":{"version":3,"names":["name","data","mode","loginForm","account","password","role","remember","registerForm","confirmPassword","department","agree","showAgreement","computed","isLogin","methods","toast","type","msg","$message","e","window","ElMessage","alert","handleLogin","roleStr","http","$axios","resp","post","accountNo","r","code","message","token","user","roleId","undefined","storeUser","localStorage","setItem","JSON","stringify","redirect","$route","query","$router","replace","response","handleRegister"],"sources":["D:\\code\\SpringBoot-Vue-SSRMS\\ssrms-web\\src\\components\\LoginRegister.vue"],"sourcesContent":["<template>\r\n  <!-- 这一层就是中间的白色大卡片内部结构，左右两边布局不变 -->\r\n  <div class=\"auth-wrapper\">\r\n    <!-- 左侧：登录/注册表单 -->\r\n    <div class=\"auth-left\">\r\n      <!-- 顶部 logo -->\r\n      <div class=\"auth-logo-row\">\r\n        <div class=\"logo-icon\">\r\n          <span class=\"logo-initial\">自</span>\r\n        </div>\r\n        <div class=\"logo-text\">\r\n          <div class=\"logo-title\">自习室预约系统</div>\r\n          <div class=\"logo-subtitle\">Study Room Reservation</div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"auth-card\">\r\n        <!-- 登录 / 注册 切换按钮 -->\r\n        <div class=\"mode-toggle\">\r\n          <button\r\n              type=\"button\"\r\n              class=\"mode-btn\"\r\n              :class=\"{ active: isLogin }\"\r\n              @click=\"switchMode('login')\"\r\n          >\r\n            登录\r\n          </button>\r\n          <button\r\n              type=\"button\"\r\n              class=\"mode-btn\"\r\n              :class=\"{ active: !isLogin }\"\r\n              @click=\"switchMode('register')\"\r\n          >\r\n            注册\r\n          </button>\r\n        </div>\r\n\r\n        <h1 class=\"auth-title\">\r\n          {{ isLogin ? '欢迎回来' : '欢迎加入' }}\r\n        </h1>\r\n        <p class=\"auth-desc\">\r\n          {{\r\n            isLogin\r\n                ? '登录到您的账号，开始管理自习预约。'\r\n                : '填写以下信息，完成自习室预约系统的账号注册。'\r\n          }}\r\n        </p>\r\n\r\n        <!-- 登录表单 -->\r\n        <form v-if=\"isLogin\" class=\"auth-form\" @submit.prevent=\"handleLogin\">\r\n          <div class=\"form-group\">\r\n            <label class=\"form-label\">账号（学号 / 工号）</label>\r\n            <input\r\n                v-model=\"loginForm.account\"\r\n                class=\"form-input\"\r\n                type=\"text\"\r\n                placeholder=\"请输入学号或工号\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label class=\"form-label\">密码</label>\r\n            <input\r\n                v-model=\"loginForm.password\"\r\n                class=\"form-input\"\r\n                type=\"password\"\r\n                placeholder=\"请输入密码\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"form-group form-inline\">\r\n            <span class=\"form-label-inline\">登录身份</span>\r\n            <label class=\"radio-label\">\r\n              <input\r\n                  type=\"radio\"\r\n                  value=\"student\"\r\n                  v-model=\"loginForm.role\"\r\n              />\r\n              学生用户\r\n            </label>\r\n            <label class=\"radio-label\">\r\n              <input\r\n                  type=\"radio\"\r\n                  value=\"admin\"\r\n                  v-model=\"loginForm.role\"\r\n              />\r\n              管理员\r\n            </label>\r\n          </div>\r\n\r\n          <div class=\"form-helper\">\r\n            <label class=\"check-label\">\r\n              <input type=\"checkbox\" v-model=\"loginForm.remember\" />\r\n              记住登录状态\r\n            </label>\r\n            <button\r\n                type=\"button\"\r\n                class=\"link-btn\"\r\n                @click.prevent=\"handleForgot\"\r\n            >\r\n              忘记密码？\r\n            </button>\r\n          </div>\r\n\r\n          <button type=\"submit\" class=\"primary-btn\">\r\n            登录\r\n          </button>\r\n\r\n          <div class=\"bottom-tip\">\r\n            还没有账号？\r\n            <button\r\n                type=\"button\"\r\n                class=\"link-btn\"\r\n                @click=\"switchMode('register')\"\r\n            >\r\n              立即注册\r\n            </button>\r\n          </div>\r\n\r\n\r\n        </form>\r\n\r\n        <!-- 注册表单 -->\r\n        <form v-else class=\"auth-form\" @submit.prevent=\"handleRegister\">\r\n          <div class=\"form-group\">\r\n            <label class=\"form-label\">姓名</label>\r\n            <input\r\n                v-model=\"registerForm.name\"\r\n                class=\"form-input\"\r\n                type=\"text\"\r\n                placeholder=\"请输入姓名\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label class=\"form-label\">学号 / 工号</label>\r\n            <input\r\n                v-model=\"registerForm.account\"\r\n                class=\"form-input\"\r\n                type=\"text\"\r\n                placeholder=\"请输入学号或工号\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label class=\"form-label\">设置密码</label>\r\n            <input\r\n                v-model=\"registerForm.password\"\r\n                class=\"form-input\"\r\n                type=\"password\"\r\n                placeholder=\"请输入密码\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label class=\"form-label\">确认密码</label>\r\n            <input\r\n                v-model=\"registerForm.confirmPassword\"\r\n                class=\"form-input\"\r\n                type=\"password\"\r\n                placeholder=\"请再次输入密码\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"form-group form-inline\">\r\n            <span class=\"form-label-inline\">注册身份</span>\r\n            <label class=\"radio-label\">\r\n              <input\r\n                  type=\"radio\"\r\n                  value=\"student\"\r\n                  v-model=\"registerForm.role\"\r\n              />\r\n              学生用户\r\n            </label>\r\n            <label class=\"radio-label\">\r\n              <input\r\n                  type=\"radio\"\r\n                  value=\"admin\"\r\n                  v-model=\"registerForm.role\"\r\n              />\r\n              管理员\r\n            </label>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label class=\"form-label\">学院 / 专业（可选）</label>\r\n            <input\r\n                v-model=\"registerForm.department\"\r\n                class=\"form-input\"\r\n                type=\"text\"\r\n                placeholder=\"例如：信息学院 · 计算机科学与技术\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"form-helper\">\r\n            <label class=\"check-label\">\r\n              <input type=\"checkbox\" v-model=\"registerForm.agree\" />\r\n              已阅读并同意\r\n              <button\r\n                  type=\"button\"\r\n                  class=\"link-btn\"\r\n                  @click.prevent=\"handleShowAgreement\"\r\n              >\r\n                《用户协议》\r\n              </button>\r\n            </label>\r\n          </div>\r\n\r\n          <button type=\"submit\" class=\"primary-btn\">\r\n            注册\r\n          </button>\r\n\r\n          <div class=\"bottom-tip\">\r\n            已有账号？\r\n            <button\r\n                type=\"button\"\r\n                class=\"link-btn\"\r\n                @click=\"switchMode('login')\"\r\n            >\r\n              直接登录\r\n            </button>\r\n          </div>\r\n\r\n        </form>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 右侧：品牌 / 文案区域 -->\r\n    <div class=\"auth-right\">\r\n      <div class=\"slogan-block\">\r\n        <h2 class=\"slogan-title\">预约自习室的最佳通用解决方案</h2>\r\n        <p class=\"slogan-desc\">\r\n          统一的自习室预约平台，让学生轻松选座，管理员高效管理场地和预约。\r\n        </p>\r\n        <ul class=\"slogan-list\">\r\n          <li>· 支持多校区、多楼栋、多自习室统一管理</li>\r\n          <li>· 支持预约、签到、违约处理与信用分统计</li>\r\n          <li>· 预留 API 接口，方便与学校现有系统对接</li>\r\n        </ul>\r\n        <div class=\"slogan-sign\">—— SSRMS</div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 用户协议弹窗 -->\r\n    <div v-if=\"showAgreement\" class=\"agreement-mask\" @click.self=\"closeAgreement\">\r\n      <div class=\"agreement-dialog\">\r\n        <div class=\"agreement-header\">\r\n          <h3 class=\"agreement-title\">自习室预约系统用户协议</h3>\r\n          <button class=\"agreement-close\" type=\"button\" @click=\"closeAgreement\">\r\n            ×\r\n          </button>\r\n        </div>\r\n        <div class=\"agreement-body\">\r\n          <div class=\"agreement-scroll\">\r\n            <p>欢迎使用自习室预约系统（以下简称“本系统”）。为了保障您的合法权益，请在注册或使用本系统前，仔细阅读并充分理解本协议的全部内容。一旦您点击注册或继续使用本系统，即视为您已阅读、理解并同意本协议的全部条款。</p>\r\n\r\n            <h4>一、账号注册与使用</h4>\r\n            <p>1. 您承诺所填写的姓名、学号/工号、学院等信息真实、准确、完整，不得冒用他人身份信息进行注册。</p>\r\n            <p>2. 您应妥善保管账号和密码，不得将账号转借、出租或转让给他人使用。因个人保管不善导致的账号安全问题，由您自行承担责任。</p>\r\n            <p>3. 若发现账号存在异常登录或被他人非法使用等情况，应及时联系系统管理员处理。</p>\r\n\r\n            <h4>二、自习室预约与使用规范</h4>\r\n            <p>1. 您应按照系统规则进行预约，包括但不限于：可预约时段、次数限制、签到/签退时间等。</p>\r\n            <p>2. 预约成功后，如因特殊情况无法到场，您应在规定时间内通过系统取消预约，避免影响其他同学使用座位。</p>\r\n            <p>3. 未按时到场且未取消预约的行为，将视为“违约”，学校或系统有权根据违约次数采取限制措施，如：暂时禁止预约、降低信用分等。</p>\r\n            <p>4. 在自习室内，您应遵守学校相关规章制度，保持环境安静、整洁，不得占座、不文明占用公共资源。</p>\r\n\r\n            <h4>三、信用管理与违约处理</h4>\r\n            <p>1. 本系统可能会结合您的签到情况、违约情况等，对您的信用状况进行记录与统计，仅用于校内管理和服务优化。</p>\r\n            <p>2. 对于多次恶意违约、恶意占用座位或其他严重影响公共秩序的行为，学校有权采取包括但不限于暂停账号、限制预约、自习室使用资格等管理措施。</p>\r\n\r\n            <h4>四、个人信息保护</h4>\r\n            <p>1. 本系统在校内部署，所采集的个人信息主要用于身份识别、预约管理、统计分析与服务改进等用途，不会用于与本系统无关的其他目的。</p>\r\n            <p>2. 未经您的授权或学校管理需要，本系统不会对外公开或向第三方提供您的个人信息，但法律法规另有规定或行政、司法机关依法提出要求的除外。</p>\r\n\r\n            <h4>五、系统维护与服务变更</h4>\r\n            <p>1. 为保证系统正常运行，本系统可能会不定期进行维护、升级或功能调整，期间可能会对您的使用造成影响，我们将尽量通过公告等方式提前告知。</p>\r\n            <p>2. 因不可抗力、网络故障、设备故障等非本系统可合理控制的原因导致的服务中断或数据异常，本系统将协助排查并尽力恢复，但在法律允许范围内不承担由此产生的间接损失。</p>\r\n\r\n            <h4>六、违约责任与适用法律</h4>\r\n            <p>1. 如您违反本协议或利用本系统从事违法违规行为，本系统有权依据学校相关规定限制或终止您对本系统的使用，并保留追究相关责任的权利。</p>\r\n            <p>2. 本协议的订立、生效、履行与解释，适用中华人民共和国现行法律以及学校相关管理制度。</p>\r\n\r\n            <h4>七、其他</h4>\r\n            <p>1. 本系统有权根据管理需要对本协议内容进行合理修订，修订后的协议将通过系统页面或公告形式提示，您继续使用本系统即视为同意修改后的内容。</p>\r\n            <p>2. 如您对本协议条款存在疑问或对使用本系统的过程中有任何建议，可通过学校公布的联系方式与系统管理员取得联系。</p>\r\n\r\n            <p>若您不同意本协议中的任何条款，请立即停止注册或使用本系统；若您点击“我已阅读并同意”或继续使用本系统，即视为您已充分理解并自愿接受本协议的全部内容。</p>\r\n          </div>\r\n        </div>\r\n        <div class=\"agreement-footer\">\r\n          <button type=\"button\" class=\"primary-btn primary-btn-small\" @click=\"agreeAndClose\">\r\n            我已阅读并同意\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'LoginRegister',\r\n  data () {\r\n    return {\r\n      mode: 'login', // 'login' 或 'register'\r\n      loginForm: {\r\n        account: '',\r\n        password: '',\r\n        role: 'student',\r\n        remember: true\r\n      },\r\n      registerForm: {\r\n        name: '',\r\n        account: '',\r\n        password: '',\r\n        confirmPassword: '',\r\n        role: 'student',\r\n        department: '',\r\n        agree: false\r\n      },\r\n      showAgreement: false\r\n    }\r\n  },\r\n  computed: {\r\n    isLogin () {\r\n      return this.mode === 'login'\r\n    }\r\n  },\r\n  methods: {\r\n    // 兼容 ElementUI / ElementPlus / 没装消息组件\r\n    toast (type, msg) {\r\n      try {\r\n        // ElementUI: this.$message\r\n        if (this.$message) return this.$message[type](msg)\r\n      } catch (e) {}\r\n      try {\r\n        // ElementPlus: ElMessage（如果你是按需引入，可自行 import）\r\n        if (window.ElMessage) return window.ElMessage[type](msg)\r\n      } catch (e) {}\r\n      alert(msg)\r\n    },\r\n\r\n    async handleLogin () {\r\n      if (!this.loginForm.account || !this.loginForm.password) {\r\n        this.toast('warning', '请填写账号和密码')\r\n        return\r\n      }\r\n\r\n      // 你页面如果有“学生/管理员”选择，建议用字符串 role：student/admin\r\n      const roleStr = this.loginForm.role || 'student'\r\n\r\n      try {\r\n        // 如果你项目里已经把 axios 挂到 this.$axios，就用它；否则你就 import axios\r\n        const http = this.$axios\r\n\r\n        const resp = await http.post('http://localhost:8090/auth/login', {\r\n          accountNo: this.loginForm.account,\r\n          password: this.loginForm.password,\r\n          role: roleStr\r\n        })\r\n\r\n        const r = resp.data\r\n        if (!r || r.code !== 200) {\r\n          this.toast('error', (r && r.message) || '登录失败')\r\n          return\r\n        }\r\n\r\n        const token = r.data && r.data.token\r\n        const user = (r.data && r.data.user) ? r.data.user : {}\r\n\r\n        // 关键：保证 roleId 存在（0 管理员 / 1 学生）\r\n        const roleId = (user.roleId !== undefined && user.roleId !== null)\r\n            ? user.roleId\r\n            : ((user.role === 'admin' || roleStr === 'admin') ? 0 : 1)\r\n\r\n        const storeUser = { ...user, roleId }\r\n\r\n        localStorage.setItem('ssrmsUser', JSON.stringify(storeUser))\r\n        if (token) localStorage.setItem('ssrmsToken', token)\r\n\r\n        this.toast('success', '登录成功')\r\n\r\n        // 支持守卫带回跳转\r\n        const redirect = this.$route?.query?.redirect\r\n        if (redirect) this.$router.replace(redirect)\r\n        else this.$router.replace(roleId === 0 ? '/admin' : '/user')\r\n      } catch (e) {\r\n        this.toast('error', e?.response?.data?.message || '登录请求失败，请检查后端是否启动/端口是否正确')\r\n      }\r\n    },\r\n\r\n    // 注册先不实现：保留 UI + 校验 + 提示\r\n    handleRegister () {\r\n      this.toast('info', '注册功能暂未接入后端接口，后续再实现')\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n"],"mappings":";AA6SA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,IAAI,EAAE,OAAO;MAAE;MACfC,SAAS,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZC,IAAI,EAAE,SAAS;QACfC,QAAQ,EAAE;MACZ,CAAC;MACDC,YAAY,EAAE;QACZR,IAAI,EAAE,EAAE;QACRI,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZI,eAAe,EAAE,EAAE;QACnBH,IAAI,EAAE,SAAS;QACfI,UAAU,EAAE,EAAE;QACdC,KAAK,EAAE;MACT,CAAC;MACDC,aAAa,EAAE;IACjB;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,OAAMA,CAAA,EAAK;MACT,OAAO,IAAI,CAACZ,IAAG,KAAM,OAAM;IAC7B;EACF,CAAC;EACDa,OAAO,EAAE;IACP;IACAC,KAAIA,CAAGC,IAAI,EAAEC,GAAG,EAAE;MAChB,IAAI;QACF;QACA,IAAI,IAAI,CAACC,QAAQ,EAAE,OAAO,IAAI,CAACA,QAAQ,CAACF,IAAI,CAAC,CAACC,GAAG;MACnD,EAAE,OAAOE,CAAC,EAAE,CAAC;MACb,IAAI;QACF;QACA,IAAIC,MAAM,CAACC,SAAS,EAAE,OAAOD,MAAM,CAACC,SAAS,CAACL,IAAI,CAAC,CAACC,GAAG;MACzD,EAAE,OAAOE,CAAC,EAAE,CAAC;MACbG,KAAK,CAACL,GAAG;IACX,CAAC;IAED,MAAMM,WAAUA,CAAA,EAAK;MACnB,IAAI,CAAC,IAAI,CAACrB,SAAS,CAACC,OAAM,IAAK,CAAC,IAAI,CAACD,SAAS,CAACE,QAAQ,EAAE;QACvD,IAAI,CAACW,KAAK,CAAC,SAAS,EAAE,UAAU;QAChC;MACF;;MAEA;MACA,MAAMS,OAAM,GAAI,IAAI,CAACtB,SAAS,CAACG,IAAG,IAAK,SAAQ;MAE/C,IAAI;QACF;QACA,MAAMoB,IAAG,GAAI,IAAI,CAACC,MAAK;QAEvB,MAAMC,IAAG,GAAI,MAAMF,IAAI,CAACG,IAAI,CAAC,kCAAkC,EAAE;UAC/DC,SAAS,EAAE,IAAI,CAAC3B,SAAS,CAACC,OAAO;UACjCC,QAAQ,EAAE,IAAI,CAACF,SAAS,CAACE,QAAQ;UACjCC,IAAI,EAAEmB;QACR,CAAC;QAED,MAAMM,CAAA,GAAIH,IAAI,CAAC3B,IAAG