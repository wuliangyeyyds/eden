{"ast":null,"code":"import UserHeader from './UserHeader.vue';\nimport UserAside from './UserAside.vue';\nimport UserHome from './UserHome.vue';\nexport default {\n  name: 'MainIndex',\n  components: {\n    AppHeader: UserHeader,\n    AppAside: UserAside,\n    AppHome: UserHome\n  },\n  data() {\n    return {\n      // 默认首页\n      currentPage: 'home'\n    };\n  },\n  methods: {\n    changePage(page) {\n      this.currentPage = page;\n    },\n    handleLogout() {\n      // 1. 清掉当前登录用户的信息\n      localStorage.removeItem('ssrmsUser');\n      // 如果之后你有 token 之类，也可以一起删：\n      // localStorage.removeItem('ssrmsToken')\n\n      // 2. 回到登录页，用 replace 避免浏览器“后退”再回到 /user\n      this.$router.replace('/login');\n    }\n  }\n};","map":{"version":3,"names":["UserHeader","UserAside","UserHome","name","components","AppHeader","AppAside","AppHome","data","currentPage","methods","changePage","page","handleLogout","localStorage","removeItem","$router","replace"],"sources":["D:\\code\\SpringBoot-Vue-SSRMS\\ssrms-web\\src\\components\\UserIndex.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-wrapper\">\r\n    <!-- 左侧导航 -->\r\n    <AppAside\r\n        :current-page=\"currentPage\"\r\n        @change-page=\"changePage\"\r\n    />\r\n\r\n    <!-- 右侧区域：标题栏 + 主内容 -->\r\n    <div class=\"main-area\">\r\n      <AppHeader\r\n          :current-page=\"currentPage\"\r\n          @logout=\"handleLogout\"\r\n      />\r\n      <div class=\"content-area\">\r\n        <AppHome\r\n            :current-page=\"currentPage\"\r\n            @change-page=\"changePage\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserHeader from './UserHeader.vue'\r\nimport UserAside from './UserAside.vue'\r\nimport UserHome from './UserHome.vue'\r\n\r\nexport default {\r\n  name: 'MainIndex',\r\n  components: {\r\n    AppHeader: UserHeader,\r\n    AppAside: UserAside,\r\n    AppHome: UserHome\r\n  },\r\n  data () {\r\n    return {\r\n      // 默认首页\r\n      currentPage: 'home'\r\n    }\r\n  },\r\n  methods: {\r\n    changePage (page) {\r\n      this.currentPage = page\r\n    },\r\n    handleLogout () {\r\n      // 1. 清掉当前登录用户的信息\r\n      localStorage.removeItem('ssrmsUser')\r\n      // 如果之后你有 token 之类，也可以一起删：\r\n      // localStorage.removeItem('ssrmsToken')\r\n\r\n      // 2. 回到登录页，用 replace 避免浏览器“后退”再回到 /user\r\n      this.$router.replace('/login')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 整体：左侧导航 + 右侧主区域 */\r\n.app-wrapper {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  background-color: #f5f7fb;\r\n}\r\n\r\n/* 右侧主区域：上标题栏 + 下内容 */\r\n.main-area {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n/* 右侧主内容区域，留一点内边距，让卡片更贴近底部 */\r\n.content-area {\r\n  flex: 1;\r\n  display: flex;\r\n  padding: 8px 24px 20px 24px; /* 底部 padding 比较小一点 */\r\n  box-sizing: border-box;\r\n}\r\n\r\n@media (max-width: 900px) {\r\n  .app-wrapper {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .content-area {\r\n    padding: 8px 12px 16px 12px;\r\n  }\r\n}\r\n</style>"],"mappings":"AAyBA,OAAOA,UAAS,MAAO,kBAAiB;AACxC,OAAOC,SAAQ,MAAO,iBAAgB;AACtC,OAAOC,QAAO,MAAO,gBAAe;AAEpC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVC,SAAS,EAAEL,UAAU;IACrBM,QAAQ,EAAEL,SAAS;IACnBM,OAAO,EAAEL;EACX,CAAC;EACDM,IAAGA,CAAA,EAAK;IACN,OAAO;MACL;MACAC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAASA,CAAGC,IAAI,EAAE;MAChB,IAAI,CAACH,WAAU,GAAIG,IAAG;IACxB,CAAC;IACDC,YAAWA,CAAA,EAAK;MACd;MACAC,YAAY,CAACC,UAAU,CAAC,WAAW;MACnC;MACA;;MAEA;MACA,IAAI,CAACC,OAAO,CAACC,OAAO,CAAC,QAAQ;IAC/B;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}