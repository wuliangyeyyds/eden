{"ast":null,"code":"import { ElMessageBox, ElMessage } from 'element-plus';\nimport UserHeader from './UserHeader.vue';\nimport UserAside from './UserAside.vue';\nimport UserHome from './UserHome.vue';\nexport default {\n  name: 'MainIndex',\n  components: {\n    AppHeader: UserHeader,\n    AppAside: UserAside,\n    AppHome: UserHome\n  },\n  data() {\n    return {\n      currentPage: 'home'\n    };\n  },\n  methods: {\n    changePage(page) {\n      this.currentPage = page;\n    },\n    async handleLogout() {\n      try {\n        await ElMessageBox.confirm('确定要退出登录吗？', '提示', {\n          confirmButtonText: '退出',\n          cancelButtonText: '取消',\n          type: 'warning'\n        });\n\n        // 1) 清掉登录信息（路由守卫用 ssrmsUser 的话，这个必须删）\n        localStorage.removeItem('ssrmsUser');\n        // 可选：如果你存过 token，也一起删\n        localStorage.removeItem('ssrmsToken');\n        localStorage.removeItem('token');\n        sessionStorage.removeItem('ssrmsUser');\n        sessionStorage.removeItem('ssrmsToken');\n        sessionStorage.removeItem('token');\n\n        // 2) 跳回登录页（replace 防止后退回到 /user）\n        ElMessage.success('已退出登录');\n        this.$router.replace('/login');\n      } catch (e) {\n        // 用户点了“取消”就啥也不做\n      }\n    }\n  }\n};","map":{"version":3,"names":["ElMessageBox","ElMessage","UserHeader","UserAside","UserHome","name","components","AppHeader","AppAside","AppHome","data","currentPage","methods","changePage","page","handleLogout","confirm","confirmButtonText","cancelButtonText","type","localStorage","removeItem","sessionStorage","success","$router","replace","e"],"sources":["D:\\code\\SpringBoot-Vue-SSRMS\\ssrms-web\\src\\components\\UserIndex.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-wrapper\">\r\n    <!-- 左侧导航 -->\r\n    <AppAside\r\n        :current-page=\"currentPage\"\r\n        @change-page=\"changePage\"\r\n    />\r\n\r\n    <!-- 右侧区域：标题栏 + 主内容 -->\r\n    <div class=\"main-area\">\r\n      <AppHeader\r\n          :current-page=\"currentPage\"\r\n          @logout=\"handleLogout\"\r\n      />\r\n      <div class=\"content-area\">\r\n        <AppHome\r\n            :current-page=\"currentPage\"\r\n            @change-page=\"changePage\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ElMessageBox, ElMessage } from 'element-plus'\r\nimport UserHeader from './UserHeader.vue'\r\nimport UserAside from './UserAside.vue'\r\nimport UserHome from './UserHome.vue'\r\n\r\nexport default {\r\n  name: 'MainIndex',\r\n  components: {\r\n    AppHeader: UserHeader,\r\n    AppAside: UserAside,\r\n    AppHome: UserHome\r\n  },\r\n  data () {\r\n    return {\r\n      currentPage: 'home'\r\n    }\r\n  },\r\n  methods: {\r\n    changePage (page) {\r\n      this.currentPage = page\r\n    },\r\n\r\n    async handleLogout () {\r\n      try {\r\n        await ElMessageBox.confirm(\r\n            '确定要退出登录吗？',\r\n            '提示',\r\n            {\r\n              confirmButtonText: '退出',\r\n              cancelButtonText: '取消',\r\n              type: 'warning'\r\n            }\r\n        )\r\n\r\n        // 1) 清掉登录信息（路由守卫用 ssrmsUser 的话，这个必须删）\r\n        localStorage.removeItem('ssrmsUser')\r\n        // 可选：如果你存过 token，也一起删\r\n        localStorage.removeItem('ssrmsToken')\r\n        localStorage.removeItem('token')\r\n        sessionStorage.removeItem('ssrmsUser')\r\n        sessionStorage.removeItem('ssrmsToken')\r\n        sessionStorage.removeItem('token')\r\n\r\n        // 2) 跳回登录页（replace 防止后退回到 /user）\r\n        ElMessage.success('已退出登录')\r\n        this.$router.replace('/login')\r\n      } catch (e) {\r\n        // 用户点了“取消”就啥也不做\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 整体：左侧导航 + 右侧主区域 */\r\n.app-wrapper {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  background-color: #f5f7fb;\r\n}\r\n\r\n/* 右侧主区域：上标题栏 + 下内容 */\r\n.main-area {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n/* 右侧主内容区域，留一点内边距，让卡片更贴近底部 */\r\n.content-area {\r\n  flex: 1;\r\n  display: flex;\r\n  padding: 8px 24px 20px 24px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n@media (max-width: 900px) {\r\n  .app-wrapper {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .content-area {\r\n    padding: 8px 12px 16px 12px;\r\n  }\r\n}\r\n</style>"],"mappings":"AAyBA,SAASA,YAAY,EAAEC,SAAQ,QAAS,cAAa;AACrD,OAAOC,UAAS,MAAO,kBAAiB;AACxC,OAAOC,SAAQ,MAAO,iBAAgB;AACtC,OAAOC,QAAO,MAAO,gBAAe;AAEpC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVC,SAAS,EAAEL,UAAU;IACrBM,QAAQ,EAAEL,SAAS;IACnBM,OAAO,EAAEL;EACX,CAAC;EACDM,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAASA,CAAGC,IAAI,EAAE;MAChB,IAAI,CAACH,WAAU,GAAIG,IAAG;IACxB,CAAC;IAED,MAAMC,YAAWA,CAAA,EAAK;MACpB,IAAI;QACF,MAAMf,YAAY,CAACgB,OAAO,CACtB,WAAW,EACX,IAAI,EACJ;UACEC,iBAAiB,EAAE,IAAI;UACvBC,gBAAgB,EAAE,IAAI;UACtBC,IAAI,EAAE;QACR,CACJ;;QAEA;QACAC,YAAY,CAACC,UAAU,CAAC,WAAW;QACnC;QACAD,YAAY,CAACC,UAAU,CAAC,YAAY;QACpCD,YAAY,CAACC,UAAU,CAAC,OAAO;QAC/BC,cAAc,CAACD,UAAU,CAAC,WAAW;QACrCC,cAAc,CAACD,UAAU,CAAC,YAAY;QACtCC,cAAc,CAACD,UAAU,CAAC,OAAO;;QAEjC;QACApB,SAAS,CAACsB,OAAO,CAAC,OAAO;QACzB,IAAI,CAACC,OAAO,CAACC,OAAO,CAAC,QAAQ;MAC/B,EAAE,OAAOC,CAAC,EAAE;QACV;MAAA;IAEJ;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}