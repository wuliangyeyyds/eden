{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AppHeader from './AppHeader.vue';\nimport AppAside from './AppAside.vue';\nimport AppHome from './AppHome.vue';\nexport default {\n  name: 'MainIndex',\n  components: {\n    AppHeader,\n    AppAside,\n    AppHome\n  },\n  // ✅ 路由会通过 props 传进 initialPage，用来区分学生首页 / 管理员首页\n  props: {\n    initialPage: {\n      type: String,\n      default: 'user-main'\n    }\n  },\n  data() {\n    return {\n      // 用 initialPage 作为当前页面的初始值\n      currentPage: this.initialPage\n    };\n  },\n  methods: {\n    // 头部导航 / 卡片点击时切换“子页面”\n    changePage(page) {\n      this.currentPage = page;\n    },\n    // 退出登录：跳回 /login\n    handleLogout() {\n      // 如果有 token，这里也可以顺便清掉\n      // localStorage.removeItem('token')\n      // localStorage.removeItem('role')\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["AppHeader","AppAside","AppHome","name","components","props","initialPage","type","String","default","data","currentPage","methods","changePage","page","handleLogout","$router","push"],"sources":["D:\\code\\SpringBoot-Vue-SSRMS\\ssrms-web\\src\\components\\MainIndex.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-wrapper\">\r\n    <!-- 头部：负责学生端 / 管理员端导航 + 退出按钮 -->\r\n    <AppHeader\r\n        :current-page=\"currentPage\"\r\n        @change-page=\"changePage\"\r\n        @logout=\"handleLogout\"\r\n    />\r\n\r\n    <!-- 中间两栏布局：左侧公告 / 概览 + 右侧主内容区域 -->\r\n    <div class=\"layout-row\">\r\n      <AppAside :current-page=\"currentPage\" />\r\n      <AppHome\r\n          :current-page=\"currentPage\"\r\n          @change-page=\"changePage\"\r\n      />\r\n    </div>\r\n\r\n    <!-- 底部 -->\r\n    <div class=\"footer\">\r\n      自习室预约管理系统 · 学生端 / 管理员端统一工作台 · 仅供课程设计使用\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AppHeader from './AppHeader.vue'\r\nimport AppAside from './AppAside.vue'\r\nimport AppHome from './AppHome.vue'\r\n\r\nexport default {\r\n  name: 'MainIndex',\r\n  components: {\r\n    AppHeader,\r\n    AppAside,\r\n    AppHome\r\n  },\r\n  // ✅ 路由会通过 props 传进 initialPage，用来区分学生首页 / 管理员首页\r\n  props: {\r\n    initialPage: {\r\n      type: String,\r\n      default: 'user-main'\r\n    }\r\n  },\r\n  data () {\r\n    return {\r\n      // 用 initialPage 作为当前页面的初始值\r\n      currentPage: this.initialPage\r\n    }\r\n  },\r\n  methods: {\r\n    // 头部导航 / 卡片点击时切换“子页面”\r\n    changePage (page) {\r\n      this.currentPage = page\r\n    },\r\n    // 退出登录：跳回 /login\r\n    handleLogout () {\r\n      // 如果有 token，这里也可以顺便清掉\r\n      // localStorage.removeItem('token')\r\n      // localStorage.removeItem('role')\r\n      this.$router.push('/login')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.app-wrapper {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n/* 中间两栏整体容器 */\r\n.layout-row {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 20px 20px 40px 20px;\r\n}\r\n\r\n.footer {\r\n  padding: 16px;\r\n  text-align: center;\r\n  background: #1f2937;\r\n  color: #9ca3af;\r\n  font-size: 12px;\r\n}\r\n\r\n/* 小屏时上下堆叠 */\r\n@media screen and (max-width: 700px) {\r\n  .layout-row {\r\n    flex-direction: column;\r\n    padding: 10px;\r\n  }\r\n}\r\n</style>"],"mappings":";AA0BA,OAAOA,SAAQ,MAAO,iBAAgB;AACtC,OAAOC,QAAO,MAAO,gBAAe;AACpC,OAAOC,OAAM,MAAO,eAAc;AAElC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVJ,SAAS;IACTC,QAAQ;IACRC;EACF,CAAC;EACD;EACAG,KAAK,EAAE;IACLC,WAAW,EAAE;MACXC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,IAAGA,CAAA,EAAK;IACN,OAAO;MACL;MACAC,WAAW,EAAE,IAAI,CAACL;IACpB;EACF,CAAC;EACDM,OAAO,EAAE;IACP;IACAC,UAASA,CAAGC,IAAI,EAAE;MAChB,IAAI,CAACH,WAAU,GAAIG,IAAG;IACxB,CAAC;IACD;IACAC,YAAWA,CAAA,EAAK;MACd;MACA;MACA;MACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ;IAC5B;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}