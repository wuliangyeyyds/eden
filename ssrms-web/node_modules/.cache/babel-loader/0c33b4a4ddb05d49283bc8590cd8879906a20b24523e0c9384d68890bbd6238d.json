{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\nimport { ElMessage } from 'element-plus';\nexport default {\n  name: 'LoginRegister',\n  data() {\n    return {\n      mode: 'login',\n      // 'login' 或 'register'\n      loginForm: {\n        account: '',\n        password: '',\n        role: 'student',\n        remember: true\n      },\n      registerForm: {\n        account: '',\n        studentNo: '',\n        name: '',\n        password: '',\n        confirmPassword: '',\n        role: 'student',\n        agree: false\n      },\n      showAgreement: false,\n      registerErrors: {\n        account: '',\n        password: '',\n        confirmPassword: ''\n      },\n      accountTimer: null\n    };\n  },\n  computed: {\n    isLogin() {\n      return this.mode === 'login';\n    }\n  },\n  methods: {\n    toast(type, msg) {\n      ElMessage({\n        type,\n        message: msg\n      });\n    },\n    // ✅ 补齐：切换登录/注册\n    switchMode(next) {\n      this.mode = next;\n\n      // 切换时清掉注册提示（避免残留）\n      this.registerErrors.account = '';\n      this.registerErrors.confirmPassword = '';\n\n      // 取消账号查重的延迟定时器\n      if (this.accountTimer) {\n        clearTimeout(this.accountTimer);\n        this.accountTimer = null;\n      }\n    },\n    // ✅ 补齐：忘记密码（先占位）\n    handleForgot() {\n      this.toast('info', '忘记密码功能暂未实现，请联系管理员重置密码');\n    },\n    // ✅ 补齐：用户协议弹窗控制\n    handleShowAgreement() {\n      this.showAgreement = true;\n    },\n    closeAgreement() {\n      this.showAgreement = false;\n    },\n    agreeAndClose() {\n      this.registerForm.agree = true;\n      this.showAgreement = false;\n    },\n    async handleLogin() {\n      if (!this.loginForm.account || !this.loginForm.password) {\n        this.toast('warning', '请填写账号和密码');\n        return;\n      }\n      const roleStr = this.loginForm.role || 'student';\n      try {\n        const http = this.$axios;\n        const resp = await http.post('/auth/login', {\n          accountNo: this.loginForm.account,\n          password: this.loginForm.password,\n          role: roleStr\n        });\n        const r = resp.data;\n        if (!r || r.code !== 200) {\n          this.toast('error', r && r.message || '登录失败');\n          return;\n        }\n        const token = r.data && r.data.token;\n        const user = r.data && r.data.user ? r.data.user : {};\n\n        // ✅ 关键：保证 roleId 存在（0 管理员 / 1 学生）\n        const roleId = user.roleId !== undefined && user.roleId !== null ? user.roleId : user.role === 'admin' || roleStr === 'admin' ? 0 : 1;\n        const storeUser = {\n          ...user,\n          roleId\n        };\n\n        // 先统一写 localStorage（因为你的路由守卫目前只读 localStorage）\n        localStorage.setItem('ssrmsUser', JSON.stringify(storeUser));\n        if (token) localStorage.setItem('ssrmsToken', token);\n        this.toast('success', '登录成功');\n        const redirect = this.$route?.query?.redirect;\n        if (redirect) this.$router.replace(redirect);else this.$router.replace(roleId === 0 ? '/admin' : '/user');\n      } catch (e) {\n        this.toast('error', e?.response?.data?.message || '登录请求失败，请检查后端是否启动/端口是否正确');\n      }\n    },\n    async handleRegister() {\n      // 先做一次最终校验（确保红字最新）\n      await this.checkAccountExists();\n      this.checkPasswordMatch();\n      if (this.registerErrors.account) {\n        this.toast('error', this.registerErrors.account);\n        return;\n      }\n      if (this.registerErrors.confirmPassword) {\n        this.toast('error', this.registerErrors.confirmPassword);\n        return;\n      }\n\n      // 基本必填校验\n      if (!this.registerForm.account || !this.registerForm.studentNo || !this.registerForm.name) {\n        this.toast('warning', '请填写账号、学号、姓名');\n        return;\n      }\n      if (!this.registerForm.password || !this.registerForm.confirmPassword) {\n        this.toast('warning', '请设置并确认密码');\n        return;\n      }\n      if (!this.registerForm.agree) {\n        this.toast('warning', '请先阅读并同意用户协议');\n        return;\n      }\n\n      // 调后端注册\n      const resp = await this.$axios.post('/user/register', {\n        account: this.registerForm.account,\n        name: this.registerForm.name,\n        studentNo: this.registerForm.studentNo,\n        password: this.registerForm.password,\n        confirmPassword: this.registerForm.confirmPassword,\n        role: this.registerForm.role // \"student\" / \"admin\"\n      });\n      const r = resp.data;\n      if (!r || r.code !== 200) {\n        const msg = r && r.msg || '注册失败';\n        // 后端返回的两类错误，顺便同步到红字\n        if (msg.includes('账号已存在')) this.registerErrors.account = '该账号已存在';\n        if (msg.includes('密码不同')) this.registerErrors.confirmPassword = '密码不同';\n        this.toast('error', msg);\n        return;\n      }\n      this.toast('success', '注册成功，请登录');\n      this.switchMode('login');\n    },\n    scheduleCheckAccount() {\n      if (this.mode !== 'register') return;\n      clearTimeout(this.accountTimer);\n      this.accountTimer = setTimeout(() => {\n        this.checkAccountExists();\n      }, 350);\n    },\n    async checkAccountExists() {\n      if (this.mode !== 'register') return;\n      const acc = (this.registerForm.account || '').trim();\n      if (!acc) {\n        this.registerErrors.account = '';\n        return;\n      }\n      try {\n        const res = await this.$axios.get('/user/check-account', {\n          params: {\n            account: acc\n          }\n        });\n        const exists = res?.data?.data === true;\n        this.registerErrors.account = exists ? '该账号已存在' : '';\n      } catch (e) {\n        this.registerErrors.account = '';\n      }\n    },\n    checkPasswordMatch() {\n      const p1 = this.registerForm.password || '';\n      const p2 = this.registerForm.confirmPassword || '';\n      if (!p2) {\n        this.registerErrors.confirmPassword = '';\n        return;\n      }\n      this.registerErrors.confirmPassword = p1 !== p2 ? '密码不同' : '';\n    }\n  }\n};","map":{"version":3,"names":["ElMessage","name","data","mode","loginForm","account","password","role","remember","registerForm","studentNo","confirmPassword","agree","showAgreement","registerErrors","accountTimer","computed","isLogin","methods","toast","type","msg","message","switchMode","next","clearTimeout","handleForgot","handleShowAgreement","closeAgreement","agreeAndClose","handleLogin","roleStr","http","$axios","resp","post","accountNo","r","code","token","user","roleId","undefined","storeUser","localStorage","setItem","JSON","stringify","redirect","$route","query","$router","replace","e","response","handleRegister","checkAccountExists","checkPasswordMatch","includes","scheduleCheckAccount","setTimeout","acc","trim","res","get","params","exists","p1","p2"],"sources":["D:\\code\\SpringBoot-Vue-SSRMS\\ssrms-web\\src\\components\\LoginRegister.vue"],"sourcesContent":["<template>\r\n  <!-- ✅ 作为整页使用：加一层全屏居中容器 -->\r\n  <div class=\"login-page\">\r\n    <!-- 这一层就是中间的白色大卡片内部结构，左右两边布局不变 -->\r\n    <div class=\"auth-wrapper\">\r\n      <!-- 左侧：登录/注册表单 -->\r\n      <div class=\"auth-left\">\r\n        <!-- 顶部 logo -->\r\n        <div class=\"auth-logo-row\">\r\n          <div class=\"logo-icon\">\r\n            <span class=\"logo-initial\">自</span>\r\n          </div>\r\n          <div class=\"logo-text\">\r\n            <div class=\"logo-title\">自习室预约系统</div>\r\n            <div class=\"logo-subtitle\">Study Room Reservation</div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"auth-card\">\r\n          <!-- 登录 / 注册 切换按钮 -->\r\n          <div class=\"mode-toggle\">\r\n            <button\r\n                type=\"button\"\r\n                class=\"mode-btn\"\r\n                :class=\"{ active: isLogin }\"\r\n                @click=\"switchMode('login')\"\r\n            >\r\n              登录\r\n            </button>\r\n            <button\r\n                type=\"button\"\r\n                class=\"mode-btn\"\r\n                :class=\"{ active: !isLogin }\"\r\n                @click=\"switchMode('register')\"\r\n            >\r\n              注册\r\n            </button>\r\n          </div>\r\n\r\n          <h1 class=\"auth-title\">\r\n            {{ isLogin ? '欢迎回来' : '欢迎加入' }}\r\n          </h1>\r\n          <p class=\"auth-desc\">\r\n            {{\r\n              isLogin\r\n                  ? '登录到您的账号，开始管理自习预约。'\r\n                  : '填写以下信息，完成自习室预约系统的账号注册。'\r\n            }}\r\n          </p>\r\n\r\n          <!-- 登录表单 -->\r\n          <form v-if=\"isLogin\" class=\"auth-form\" @submit.prevent=\"handleLogin\">\r\n            <div class=\"form-group\">\r\n              <label class=\"form-label\">账号（学号 / 工号）</label>\r\n              <input\r\n                  v-model=\"loginForm.account\"\r\n                  class=\"form-input\"\r\n                  type=\"text\"\r\n                  placeholder=\"请输入学号或工号\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label class=\"form-label\">密码</label>\r\n              <input\r\n                  v-model=\"loginForm.password\"\r\n                  class=\"form-input\"\r\n                  type=\"password\"\r\n                  placeholder=\"请输入密码\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form-group form-inline\">\r\n              <span class=\"form-label-inline\">登录身份</span>\r\n              <label class=\"radio-label\">\r\n                <input type=\"radio\" value=\"student\" v-model=\"loginForm.role\" />\r\n                学生用户\r\n              </label>\r\n              <label class=\"radio-label\">\r\n                <input type=\"radio\" value=\"admin\" v-model=\"loginForm.role\" />\r\n                管理员\r\n              </label>\r\n            </div>\r\n\r\n            <div class=\"form-helper\">\r\n              <label class=\"check-label\">\r\n                <input type=\"checkbox\" v-model=\"loginForm.remember\" />\r\n                记住登录状态\r\n              </label>\r\n              <button type=\"button\" class=\"link-btn\" @click.prevent=\"handleForgot\">\r\n                忘记密码？\r\n              </button>\r\n            </div>\r\n\r\n            <button type=\"submit\" class=\"primary-btn\">登录</button>\r\n\r\n            <div class=\"bottom-tip\">\r\n              还没有账号？\r\n              <button type=\"button\" class=\"link-btn\" @click=\"switchMode('register')\">\r\n                立即注册\r\n              </button>\r\n            </div>\r\n          </form>\r\n\r\n          <!-- 注册表单（重点：必须是 v-else） -->\r\n          <form v-else class=\"auth-form\" @submit.prevent=\"handleRegister\">\r\n            <div class=\"form-group\">\r\n              <label class=\"form-label\">账号</label>\r\n              <input\r\n                  v-model=\"registerForm.account\"\r\n                  class=\"form-input\"\r\n                  type=\"text\"\r\n                  placeholder=\"请输入账号（用于登录）\"\r\n                  @input=\"scheduleCheckAccount\"\r\n                  @blur=\"checkAccountExists\"\r\n              />\r\n              <div v-if=\"registerErrors.account\" class=\"field-error\">\r\n                {{ registerErrors.account }}\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label class=\"form-label\">学号</label>\r\n              <input\r\n                  v-model=\"registerForm.studentNo\"\r\n                  class=\"form-input\"\r\n                  type=\"text\"\r\n                  placeholder=\"请输入学号\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label class=\"form-label\">姓名</label>\r\n              <input\r\n                  v-model=\"registerForm.name\"\r\n                  class=\"form-input\"\r\n                  type=\"text\"\r\n                  placeholder=\"请输入姓名\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label class=\"form-label\">设置密码</label>\r\n              <input\r\n                  v-model=\"registerForm.password\"\r\n                  class=\"form-input\"\r\n                  type=\"password\"\r\n                  placeholder=\"请输入密码\"\r\n                  @input=\"checkPasswordMatch\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label class=\"form-label\">确认密码</label>\r\n              <input\r\n                  v-model=\"registerForm.confirmPassword\"\r\n                  class=\"form-input\"\r\n                  type=\"password\"\r\n                  placeholder=\"请再次输入密码\"\r\n                  @input=\"checkPasswordMatch\"\r\n              />\r\n              <div v-if=\"registerErrors.confirmPassword\" class=\"field-error\">\r\n                {{ registerErrors.confirmPassword }}\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"form-group form-inline\">\r\n              <span class=\"form-label-inline\">注册身份</span>\r\n              <label class=\"radio-label\">\r\n                <input type=\"radio\" value=\"student\" v-model=\"registerForm.role\" />\r\n                学生用户\r\n              </label>\r\n              <label class=\"radio-label\">\r\n                <input type=\"radio\" value=\"admin\" v-model=\"registerForm.role\" />\r\n                管理员\r\n              </label>\r\n            </div>\r\n\r\n            <div class=\"form-helper\">\r\n              <label class=\"check-label\">\r\n                <input type=\"checkbox\" v-model=\"registerForm.agree\" />\r\n                已阅读并同意\r\n                <button type=\"button\" class=\"link-btn\" @click.prevent=\"handleShowAgreement\">\r\n                  《用户协议》\r\n                </button>\r\n              </label>\r\n            </div>\r\n\r\n            <button type=\"submit\" class=\"primary-btn\">注册</button>\r\n\r\n            <div class=\"bottom-tip\">\r\n              已有账号？\r\n              <button type=\"button\" class=\"link-btn\" @click=\"switchMode('login')\">\r\n                直接登录\r\n              </button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 右侧：品牌 / 文案区域 -->\r\n      <div class=\"auth-right\">\r\n        <div class=\"slogan-block\">\r\n          <h2 class=\"slogan-title\">预约自习室的最佳通用解决方案</h2>\r\n          <p class=\"slogan-desc\">\r\n            统一的自习室预约平台，让学生轻松选座，管理员高效管理场地和预约。\r\n          </p>\r\n          <ul class=\"slogan-list\">\r\n            <li>· 支持多校区、多楼栋、多自习室统一管理</li>\r\n            <li>· 支持预约、签到、违约处理与信用分统计</li>\r\n            <li>· 预留 API 接口，方便与学校现有系统对接</li>\r\n          </ul>\r\n          <div class=\"slogan-sign\">—— SSRMS</div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 用户协议弹窗 -->\r\n      <div v-if=\"showAgreement\" class=\"agreement-mask\" @click.self=\"closeAgreement\">\r\n        <div class=\"agreement-dialog\">\r\n          <div class=\"agreement-header\">\r\n            <h3 class=\"agreement-title\">自习室预约系统用户协议</h3>\r\n            <button class=\"agreement-close\" type=\"button\" @click=\"closeAgreement\">\r\n              ×\r\n            </button>\r\n          </div>\r\n          <div class=\"agreement-body\">\r\n            <div class=\"agreement-scroll\">\r\n              <p>\r\n                欢迎使用自习室预约系统（以下简称“本系统”）。为保障您的合法权益并维护自习室公共秩序，请您在注册、登录或使用本系统前仔细阅读并充分理解本协议全部内容。\r\n                您点击“注册/登录/我已阅读并同意”或实际使用本系统功能，即表示您已阅读、理解并同意接受本协议的约束。\r\n              </p>\r\n\r\n              <h4>一、定义与适用范围</h4>\r\n              <p>1. 本系统：指自习室预约系统（SSRMS）及其网页端页面、接口服务及相关功能。</p>\r\n              <p>2. 用户：指注册并使用本系统的学生用户、管理员用户等。</p>\r\n              <p>3. 适用范围：本协议适用于用户在本系统内进行注册、登录、预约、取消、签到、签退、查询记录、信用/违约查看等全部行为。</p>\r\n\r\n              <h4>二、账号注册与管理</h4>\r\n              <p>1. 用户注册应提供真实、准确、完整的信息，包括但不限于：账号、姓名、学号、密码等。</p>\r\n              <p>2. 用户应妥善保管账号与密码，不得将账号出借、出租、转让或以其他方式提供给他人使用。</p>\r\n              <p>3. 因用户自身原因导致账号泄露、被盗用或造成损失的，由用户自行承担相应责任；系统将尽力提供必要协助（如重置密码、冻结异常账号等）。</p>\r\n              <p>4. 若用户发现账号异常登录、信息被篡改或存在安全风险，应及时联系系统管理员处理。</p>\r\n\r\n              <h4>三、预约规则与使用规范</h4>\r\n              <p>1. 用户应在系统开放的规则范围内预约自习座位，包括可预约日期、时间段、次数上限、地点限制等（以系统页面提示为准）。</p>\r\n              <p>2. 预约成功后，用户应按约定时间到达自习室并在规定时间内完成签到/签退（如系统支持）。</p>\r\n              <p>3. 若用户无法按时使用座位，应在规定时间内取消预约，以便释放座位资源。</p>\r\n              <p>4. 用户在自习室内应遵守学校/场馆管理规定，保持安静、整洁，不得影响他人学习，不得恶意占座、代占、长时间离座不归等。</p>\r\n\r\n              <h4>四、违约处理与信用管理</h4>\r\n              <p>1. 为维护公共资源公平使用，本系统可能对以下行为记录为违约（示例，具体以系统规则为准）：</p>\r\n              <p>（1）预约后未按时签到/未到场且未取消；</p>\r\n              <p>（2）超过规定时间取消/取消超时；</p>\r\n              <p>（3）到场迟到超过系统允许阈值；</p>\r\n              <p>（4）其他严重影响秩序的行为。</p>\r\n              <p>2. 系统可能依据违约次数或情节采取限制措施，包括但不限于：降低信用分、限制预约次数、短期禁止预约、账号禁用等。</p>\r\n              <p>3. 用户如对违约记录有异议，可通过系统反馈渠道向管理员申诉，管理员可基于实际情况进行复核与处理。</p>\r\n\r\n              <h4>五、个人信息与数据保护</h4>\r\n              <p>1. 本系统采集的用户信息与使用记录主要用于：身份识别、预约管理、违约/信用统计、系统运营与功能优化。</p>\r\n              <p>2. 系统将采取合理的技术与管理措施保护用户信息安全，防止信息泄露、损毁或丢失。</p>\r\n              <p>3. 未经用户同意，系统不会向无关第三方公开或提供用户个人信息，但法律法规要求或行政/司法机关依法提出要求的除外。</p>\r\n\r\n              <h4>六、系统服务与免责说明</h4>\r\n              <p>1. 系统可能因维护、升级、故障、网络波动等原因出现短暂中断或功能异常，系统将尽力修复并减少影响。</p>\r\n              <p>2. 因不可抗力或非系统可控因素导致的服务中断、数据异常或损失，系统在法律允许范围内不承担间接损失责任，但会提供合理协助。</p>\r\n              <p>3. 用户应遵守法律法规与学校相关管理制度，不得利用本系统从事违法违规行为。</p>\r\n\r\n              <h4>七、协议变更与终止</h4>\r\n              <p>1. 系统有权根据管理需要或功能调整对本协议进行更新，并通过公告/页面提示等方式发布；用户继续使用本系统即视为接受更新后的协议。</p>\r\n              <p>2. 若用户违反本协议或学校相关管理制度，系统有权采取包括但不限于限制功能、冻结账号、终止服务等措施，并保留追究责任的权利。</p>\r\n\r\n              <h4>八、争议解决与适用规则</h4>\r\n              <p>1. 本协议适用中华人民共和国相关法律法规以及学校/场馆相关管理规定。</p>\r\n              <p>2. 因本协议引起的争议，双方应先友好协商；协商不成的，按学校相关规定或依法处理。</p>\r\n\r\n              <h4>九、联系与反馈</h4>\r\n              <p>如您对本协议或系统使用有任何疑问、建议或申诉，请通过系统反馈入口或联系系统管理员处理。</p>\r\n\r\n              <p style=\"margin-top: 10px;\">\r\n                提示：点击“我已阅读并同意”即表示您已理解并接受本协议全部条款。\r\n              </p>\r\n            </div>\r\n          </div>\r\n          <div class=\"agreement-footer\">\r\n            <button type=\"button\" class=\"primary-btn primary-btn-small\" @click=\"agreeAndClose\">\r\n              我已阅读并同意\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ElMessage } from 'element-plus'\r\n\r\nexport default {\r\n  name: 'LoginRegister',\r\n  data () {\r\n    return {\r\n      mode: 'login', // 'login' 或 'register'\r\n      loginForm: {\r\n        account: '',\r\n        password: '',\r\n        role: 'student',\r\n        remember: true\r\n      },\r\n      registerForm: {\r\n        account: '',\r\n        studentNo: '',\r\n        name: '',\r\n        password: '',\r\n        confirmPassword: '',\r\n        role: 'student',\r\n        agree: false\r\n      },\r\n      showAgreement: false,\r\n      registerErrors: {\r\n        account: '',\r\n        password: '',\r\n        confirmPassword: ''\r\n      },\r\n      accountTimer: null,\r\n    }\r\n  },\r\n  computed: {\r\n    isLogin () {\r\n      return this.mode === 'login'\r\n    }\r\n  },\r\n  methods: {\r\n    toast (type, msg) {\r\n      ElMessage({ type, message: msg })\r\n    },\r\n\r\n    // ✅ 补齐：切换登录/注册\r\n    switchMode (next) {\r\n      this.mode = next\r\n\r\n      // 切换时清掉注册提示（避免残留）\r\n      this.registerErrors.account = ''\r\n      this.registerErrors.confirmPassword = ''\r\n\r\n      // 取消账号查重的延迟定时器\r\n      if (this.accountTimer) {\r\n        clearTimeout(this.accountTimer)\r\n        this.accountTimer = null\r\n      }\r\n    },\r\n\r\n    // ✅ 补齐：忘记密码（先占位）\r\n    handleForgot () {\r\n      this.toast('info', '忘记密码功能暂未实现，请联系管理员重置密码')\r\n    },\r\n\r\n    // ✅ 补齐：用户协议弹窗控制\r\n    handleShowAgreement () {\r\n      this.showAgreement = true\r\n    },\r\n    closeAgreement () {\r\n      this.showAgreement = false\r\n    },\r\n    agreeAndClose () {\r\n      this.registerForm.agree = true\r\n      this.showAgreement = false\r\n    },\r\n\r\n    async handleLogin () {\r\n      if (!this.loginForm.account || !this.loginForm.password) {\r\n        this.toast('warning', '请填写账号和密码')\r\n        return\r\n      }\r\n\r\n      const roleStr = this.loginForm.role || 'student'\r\n\r\n      try {\r\n        const http = this.$axios\r\n        const resp = await http.post('/auth/login', {\r\n          accountNo: this.loginForm.account,\r\n          password: this.loginForm.password,\r\n          role: roleStr\r\n        })\r\n\r\n        const r = resp.data\r\n        if (!r || r.code !== 200) {\r\n          this.toast('error', (r && r.message) || '登录失败')\r\n          return\r\n        }\r\n\r\n        const token = r.data && r.data.token\r\n        const user = (r.data && r.data.user) ? r.data.user : {}\r\n\r\n        // ✅ 关键：保证 roleId 存在（0 管理员 / 1 学生）\r\n        const roleId = (user.roleId !== undefined && user.roleId !== null)\r\n            ? user.roleId\r\n            : ((user.role === 'admin' || roleStr === 'admin') ? 0 : 1)\r\n\r\n        const storeUser = { ...user, roleId }\r\n\r\n        // 先统一写 localStorage（因为你的路由守卫目前只读 localStorage）\r\n        localStorage.setItem('ssrmsUser', JSON.stringify(storeUser))\r\n        if (token) localStorage.setItem('ssrmsToken', token)\r\n\r\n        this.toast('success', '登录成功')\r\n\r\n        const redirect = this.$route?.query?.redirect\r\n        if (redirect) this.$router.replace(redirect)\r\n        else this.$router.replace(roleId === 0 ? '/admin' : '/user')\r\n      } catch (e) {\r\n        this.toast('error', e?.response?.data?.message || '登录请求失败，请检查后端是否启动/端口是否正确')\r\n      }\r\n    },\r\n\r\n    async handleRegister () {\r\n      // 先做一次最终校验（确保红字最新）\r\n      await this.checkAccountExists()\r\n      this.checkPasswordMatch()\r\n\r\n      if (this.registerErrors.account) {\r\n        this.toast('error', this.registerErrors.account)\r\n        return\r\n      }\r\n      if (this.registerErrors.confirmPassword) {\r\n        this.toast('error', this.registerErrors.confirmPassword)\r\n        return\r\n      }\r\n\r\n      // 基本必填校验\r\n      if (!this.registerForm.account || !this.registerForm.studentNo || !this.registerForm.name) {\r\n        this.toast('warning', '请填写账号、学号、姓名')\r\n        return\r\n      }\r\n      if (!this.registerForm.password || !this.registerForm.confirmPassword) {\r\n        this.toast('warning', '请设置并确认密码')\r\n        return\r\n      }\r\n      if (!this.registerForm.agree) {\r\n        this.toast('warning', '请先阅读并同意用户协议')\r\n        return\r\n      }\r\n\r\n      // 调后端注册\r\n      const resp = await this.$axios.post('/user/register', {\r\n        account: this.registerForm.account,\r\n        name: this.registerForm.name,\r\n        studentNo: this.registerForm.studentNo,\r\n        password: this.registerForm.password,\r\n        confirmPassword: this.registerForm.confirmPassword,\r\n        role: this.registerForm.role // \"student\" / \"admin\"\r\n      })\r\n\r\n      const r = resp.data\r\n      if (!r || r.code !== 200) {\r\n        const msg = (r && r.msg) || '注册失败'\r\n        // 后端返回的两类错误，顺便同步到红字\r\n        if (msg.includes('账号已存在')) this.registerErrors.account = '该账号已存在'\r\n        if (msg.includes('密码不同')) this.registerErrors.confirmPassword = '密码不同'\r\n        this.toast('error', msg)\r\n        return\r\n      }\r\n\r\n      this.toast('success', '注册成功，请登录')\r\n      this.switchMode('login')\r\n    },\r\n\r\n    scheduleCheckAccount () {\r\n      if (this.mode !== 'register') return\r\n      clearTimeout(this.accountTimer)\r\n      this.accountTimer = setTimeout(() => {\r\n        this.checkAccountExists()\r\n      }, 350)\r\n    },\r\n\r\n    async checkAccountExists () {\r\n      if (this.mode !== 'register') return\r\n      const acc = (this.registerForm.account || '').trim()\r\n      if (!acc) {\r\n        this.registerErrors.account = ''\r\n        return\r\n      }\r\n      try {\r\n        const res = await this.$axios.get('/user/check-account', { params: { account: acc } })\r\n        const exists = res?.data?.data === true\r\n        this.registerErrors.account = exists ? '该账号已存在' : ''\r\n      } catch (e) {\r\n        this.registerErrors.account = ''\r\n      }\r\n    },\r\n\r\n    checkPasswordMatch () {\r\n      const p1 = this.registerForm.password || ''\r\n      const p2 = this.registerForm.confirmPassword || ''\r\n      if (!p2) {\r\n        this.registerErrors.confirmPassword = ''\r\n        return\r\n      }\r\n      this.registerErrors.confirmPassword = (p1 !== p2) ? '密码不同' : ''\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n/* ✅ 整页居中（否则卡片会贴左上角） */\r\n.login-page {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 40px 16px;\r\n}\r\n\r\n/* 中间大卡片：宽度限制、圆角和阴影 */\r\n.auth-wrapper {\r\n  display: flex;\r\n  width: 1040px;\r\n  max-width: 96vw;\r\n  min-height: 540px;\r\n  max-height: calc(100vh - 80px);\r\n  border-radius: 26px;\r\n  overflow: hidden;\r\n  background-color: #ffffff;\r\n  box-shadow: 0 30px 80px rgba(15, 23, 42, 0.35);\r\n}\r\n\r\n/* 左侧区域 */\r\n.auth-left {\r\n  flex: 1;\r\n  background-color: #f9fafb;\r\n  display: flex;\r\n  flex-direction: column;\r\n  padding: 40px 56px;\r\n  overflow-y: auto;\r\n}\r\n\r\n/* 左侧滚动条样式（可选） */\r\n.auth-left::-webkit-scrollbar {\r\n  width: 6px;\r\n}\r\n.auth-left::-webkit-scrollbar-thumb {\r\n  background-color: rgba(148, 163, 184, 0.7);\r\n  border-radius: 999px;\r\n}\r\n.auth-left::-webkit-scrollbar-track {\r\n  background-color: transparent;\r\n}\r\n\r\n.auth-logo-row {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 32px;\r\n}\r\n\r\n.logo-icon {\r\n  width: 44px;\r\n  height: 44px;\r\n  border-radius: 14px;\r\n  background: #2563eb;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  color: #ffffff;\r\n  font-weight: 600;\r\n  font-size: 20px;\r\n  box-shadow: 0 10px 25px rgba(37, 99, 235, 0.4);\r\n}\r\n\r\n.logo-initial {\r\n  transform: translateY(1px);\r\n}\r\n\r\n.logo-text {\r\n  margin-left: 12px;\r\n}\r\n\r\n.logo-title {\r\n  font-size: 18px;\r\n  font-weight: 600;\r\n  color: #111827;\r\n}\r\n\r\n.logo-subtitle {\r\n  font-size: 12px;\r\n  color: #9ca3af;\r\n}\r\n\r\n.auth-card {\r\n  max-width: 420px;\r\n}\r\n\r\n/* 登录/注册 切换 */\r\n.mode-toggle {\r\n  display: inline-flex;\r\n  padding: 4px;\r\n  border-radius: 999px;\r\n  background-color: rgba(15, 23, 42, 0.04);\r\n  margin-bottom: 12px;\r\n}\r\n\r\n.mode-btn {\r\n  border: none;\r\n  background: transparent;\r\n  padding: 6px 22px;\r\n  border-radius: 999px;\r\n  font-size: 13px;\r\n  color: #6b7280;\r\n  cursor: pointer;\r\n  transition: all 0.15s ease;\r\n}\r\n\r\n.mode-btn.active {\r\n  background-color: #ffffff;\r\n  color: #111827;\r\n  box-shadow: 0 8px 20px rgba(15, 23, 42, 0.16);\r\n}\r\n\r\n/* 标题文字 */\r\n.auth-title {\r\n  margin: 4px 0 6px;\r\n  font-size: 26px;\r\n  font-weight: 700;\r\n  color: #111827;\r\n}\r\n\r\n.auth-desc {\r\n  margin: 0 0 18px;\r\n  font-size: 13px;\r\n  color: #6b7280;\r\n}\r\n\r\n/* 表单 */\r\n.auth-form {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 14px;\r\n}\r\n\r\n.form-group {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.form-label {\r\n  font-size: 13px;\r\n  color: #4b5563;\r\n  margin-bottom: 6px;\r\n}\r\n\r\n.form-label-inline {\r\n  font-size: 13px;\r\n  color: #4b5563;\r\n  margin-right: 8px;\r\n}\r\n\r\n.form-input {\r\n  border-radius: 10px;\r\n  border: 1px solid #e5e7eb;\r\n  padding: 10px 14px;\r\n  font-size: 14px;\r\n  outline: none;\r\n  background-color: #ffffff;\r\n  transition: border-color 0.15s ease, box-shadow 0.15s ease;\r\n}\r\n\r\n.form-input:focus {\r\n  border-color: #2563eb;\r\n  box-shadow: 0 0 0 1px rgba(37, 99, 235, 0.2);\r\n}\r\n\r\n.form-inline {\r\n  flex-direction: row;\r\n  align-items: center;\r\n  flex-wrap: wrap;\r\n  gap: 12px;\r\n}\r\n\r\n.radio-label {\r\n  font-size: 13px;\r\n  color: #4b5563;\r\n  display: inline-flex;\r\n  align-items: center;\r\n  gap: 4px