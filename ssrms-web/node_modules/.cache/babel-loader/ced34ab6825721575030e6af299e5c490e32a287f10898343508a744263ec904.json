{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport AdminUserManage from './AdminUserManage.vue';\nexport default {\n  name: 'AdminHome',\n  components: {\n    AdminUserManage\n  },\n  props: {\n    currentPage: {\n      type: String,\n      required: true\n    }\n  },\n  emits: ['change-page'],\n  data() {\n    return {\n      // ‰ª™Ë°®ÁõòÁ§∫‰æãÊï∞ÊçÆ\n      latestActivities: [{\n        id: 1,\n        text: 'Âº†‰∏â È¢ÑÁ∫¶‰∫Ü Âõæ‰π¶È¶Ü 401 ¬∑ 10:00-12:00',\n        type: 'book',\n        typeLabel: 'Êñ∞È¢ÑÁ∫¶',\n        time: '09:15'\n      }, {\n        id: 2,\n        text: 'ÊùéÂõõ Âú® Êú¨ÈÉ® ¬∑ 3 Ê•º 301 ÂÆåÊàêÁ≠æÂà∞',\n        type: 'sign',\n        typeLabel: 'Â∑≤Á≠æÂà∞',\n        time: '08:05'\n      }, {\n        id: 3,\n        text: 'Áéã‰∫î ÂèñÊ∂à‰∫Ü ÊòéÊó•ÁöÑÈ¢ÑÁ∫¶ÔºàÂéüÂõ†ÔºöËØæÁ®ãË∞ÉÊï¥Ôºâ',\n        type: 'cancel',\n        typeLabel: 'Â∑≤ÂèñÊ∂à',\n        time: '07:48'\n      }],\n      /* ========= È¢ÑÁ∫¶ÁÆ°ÁêÜÁõ∏ÂÖ≥ ========= */\n      reservationFilters: {\n        keyword: '',\n        room: '',\n        date: '',\n        status: ''\n      },\n      reservationAutoRefresh: false,\n      roomOptions: ['3 Ê•º 301', 'Âõæ‰π¶È¶Ü 401', 'Âõæ‰π¶È¶Ü 501'],\n      statusOptions: [{\n        label: 'Â∑≤È¢ÑÁ∫¶',\n        value: 'booked'\n      }, {\n        label: 'Â∑≤Á≠æÂà∞',\n        value: 'signed'\n      }, {\n        label: 'Â∑≤ÂèñÊ∂à',\n        value: 'canceled'\n      }, {\n        label: 'ËøùÁ∫¶',\n        value: 'violation'\n      }],\n      reservations: [{\n        id: 1,\n        code: 'Y20251204001',\n        studentNo: '20210001',\n        studentName: 'Âº†‰∏â',\n        room: '3 Ê•º 301',\n        seatNo: 'A-15',\n        date: '2025-12-05',\n        timeRange: '08:00-10:00',\n        status: 'booked',\n        remark: 'ËøüÂà∞ÈúÄ‰∫∫Â∑•Á°ÆËÆ§'\n      }, {\n        id: 2,\n        code: 'Y20251204002',\n        studentNo: '20210002',\n        studentName: 'ÊùéÂõõ',\n        room: 'Âõæ‰π¶È¶Ü 401',\n        seatNo: 'B-08',\n        date: '2025-12-05',\n        timeRange: '10:00-12:00',\n        status: 'signed',\n        remark: 'ÊåâÊó∂Á≠æÂà∞'\n      }, {\n        id: 3,\n        code: 'Y20251204003',\n        studentNo: '20210003',\n        studentName: 'Áéã‰∫î',\n        room: 'Âõæ‰π¶È¶Ü 401',\n        seatNo: 'C-07',\n        date: '2025-12-05',\n        timeRange: '14:00-16:00',\n        status: 'canceled',\n        remark: 'Â≠¶Áîü‰∏ªÂä®ÂèñÊ∂à'\n      }, {\n        id: 4,\n        code: 'Y20251204004',\n        studentNo: '20210004',\n        studentName: 'ËµµÂÖ≠',\n        room: '3 Ê•º 301',\n        seatNo: 'A-01',\n        date: '2025-12-04',\n        timeRange: '18:00-20:00',\n        status: 'violation',\n        remark: 'Êú™Âà∞Âú∫‰∏îÊú™ÂèñÊ∂à'\n      }],\n      reservationSelection: [],\n      reservationPage: 1,\n      reservationPageSize: 10\n    };\n  },\n  computed: {\n    todayText() {\n      const d = new Date();\n      const y = d.getFullYear();\n      const m = String(d.getMonth() + 1).padStart(2, '0');\n      const day = String(d.getDate()).padStart(2, '0');\n      return `${y}-${m}-${day}`;\n    },\n    filteredReservations() {\n      const {\n        keyword,\n        room,\n        date,\n        status\n      } = this.reservationFilters;\n      return this.reservations.filter(item => {\n        let ok = true;\n        if (keyword && keyword.trim()) {\n          const k = keyword.trim();\n          ok = ok && (item.studentName.includes(k) || item.studentNo.includes(k) || item.code.includes(k));\n        }\n        if (room) {\n          ok = ok && item.room === room;\n        }\n        if (date) {\n          ok = ok && item.date === date;\n        }\n        if (status) {\n          ok = ok && item.status === status;\n        }\n        return ok;\n      });\n    },\n    paginatedReservations() {\n      const start = (this.reservationPage - 1) * this.reservationPageSize;\n      return this.filteredReservations.slice(start, start + this.reservationPageSize);\n    },\n    reservationStats() {\n      const stats = {\n        total: 0,\n        booked: 0,\n        signed: 0,\n        canceled: 0,\n        violation: 0\n      };\n      stats.total = this.filteredReservations.length;\n      this.filteredReservations.forEach(item => {\n        if (item.status === 'booked') stats.booked += 1;else if (item.status === 'signed') stats.signed += 1;else if (item.status === 'canceled') stats.canceled += 1;else if (item.status === 'violation') stats.violation += 1;\n      });\n      return stats;\n    }\n  },\n  methods: {\n    emitChange(page) {\n      this.$emit('change-page', page);\n    },\n    /* ===== È¢ÑÁ∫¶ÁÆ°ÁêÜÊñπÊ≥ï ===== */\n    handleReservationSearch() {\n      console.log('ÊâßË°åÈ¢ÑÁ∫¶Êü•ËØ¢', this.reservationFilters);\n      this.reservationPage = 1;\n    },\n    resetReservationFilters() {\n      this.reservationFilters = {\n        keyword: '',\n        room: '',\n        date: '',\n        status: ''\n      };\n      this.reservationPage = 1;\n    },\n    handleSelectionChange(val) {\n      this.reservationSelection = val || [];\n    },\n    handlePageChange(page) {\n      this.reservationPage = page;\n    },\n    handlePageSizeChange(size) {\n      this.reservationPageSize = size;\n      this.reservationPage = 1;\n    },\n    getStatusTagType(status) {\n      if (status === 'booked') return 'info';\n      if (status === 'signed') return 'success';\n      if (status === 'canceled') return 'warning';\n      if (status === 'violation') return 'danger';\n      return '';\n    },\n    getStatusText(status) {\n      if (status === 'booked') return 'Â∑≤È¢ÑÁ∫¶';\n      if (status === 'signed') return 'Â∑≤Á≠æÂà∞';\n      if (status === 'canceled') return 'Â∑≤ÂèñÊ∂à';\n      if (status === 'violation') return 'ËøùÁ∫¶';\n      return status;\n    },\n    handleSign(row) {\n      console.log('Ë°•ÂΩïÁ≠æÂà∞', row);\n      row.status = 'signed';\n    },\n    handleCancel(row) {\n      console.log('ÂèñÊ∂àÈ¢ÑÁ∫¶', row);\n      row.status = 'canceled';\n    },\n    handleMarkViolation(row) {\n      console.log('Ê†áËÆ∞ËøùÁ∫¶', row);\n      row.status = 'violation';\n    },\n    handleViewDetail(row) {\n      console.log('Êü•ÁúãÈ¢ÑÁ∫¶ËØ¶ÊÉÖ', row);\n    },\n    handleBatchSign() {\n      console.log('ÊâπÈáèË°•ÂΩïÁ≠æÂà∞', this.reservationSelection);\n      this.reservationSelection.forEach(item => {\n        if (item.status === 'booked') item.status = 'signed';\n      });\n      this.reservationSelection = [];\n    },\n    handleBatchCancel() {\n      console.log('ÊâπÈáèÂèñÊ∂àÈ¢ÑÁ∫¶', this.reservationSelection);\n      this.reservationSelection.forEach(item => {\n        if (item.status === 'booked') item.status = 'canceled';\n      });\n      this.reservationSelection = [];\n    }\n  }\n};","map":{"version":3,"names":["AdminUserManage","name","components","props","currentPage","type","String","required","emits","data","latestActivities","id","text","typeLabel","time","reservationFilters","keyword","room","date","status","reservationAutoRefresh","roomOptions","statusOptions","label","value","reservations","code","studentNo","studentName","seatNo","timeRange","remark","reservationSelection","reservationPage","reservationPageSize","computed","todayText","d","Date","y","getFullYear","m","getMonth","padStart","day","getDate","filteredReservations","filter","item","ok","trim","k","includes","paginatedReservations","start","slice","reservationStats","stats","total","booked","signed","canceled","violation","length","forEach","methods","emitChange","page","$emit","handleReservationSearch","console","log","resetReservationFilters","handleSelectionChange","val","handlePageChange","handlePageSizeChange","size","getStatusTagType","getStatusText","handleSign","row","handleCancel","handleMarkViolation","handleViewDetail","handleBatchSign","handleBatchCancel"],"sources":["D:\\code\\SpringBoot-Vue-SSRMS\\ssrms-web\\src\\components\\AdminHome.vue"],"sourcesContent":["<template>\r\n  <div class=\"main\">\r\n    <!-- ÁÆ°ÁêÜÂëòÈ¶ñÈ°µ‰ª™Ë°®Áõò -->\r\n    <div v-if=\"currentPage === 'admin-home'\" class=\"dashboard-wrapper\">\r\n      <!-- ‰ªäÊó•Ê¶ÇËßà -->\r\n      <div class=\"card overview-card\">\r\n        <div class=\"overview-header\">\r\n          <div>\r\n            <h2 class=\"section-title\">‰ªäÊó•Ê¶ÇËßà</h2>\r\n            <p class=\"section-subtitle\">Âø´ÈÄü‰∫ÜËß£‰ªäÊó•È¢ÑÁ∫¶„ÄÅÁ≠æÂà∞‰∏éÂºÇÂ∏∏ÊÉÖÂÜµ„ÄÇ</p>\r\n          </div>\r\n          <div class=\"overview-extra\">\r\n            <span class=\"overview-date\">‰ªäÂ§©Ôºö{{ todayText }}</span>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"overview-grid\">\r\n          <div class=\"overview-item\">\r\n            <div class=\"overview-label\">‰ªäÊó•È¢ÑÁ∫¶ÊÄªÊï∞</div>\r\n            <div class=\"overview-value\">120</div>\r\n            <div class=\"overview-desc\">Áõ∏ÊØîÊò®Êó• ‚Üë 12%</div>\r\n          </div>\r\n          <div class=\"overview-item\">\r\n            <div class=\"overview-label\">‰ªäÊó•Â∑≤Á≠æÂà∞</div>\r\n            <div class=\"overview-value\">98</div>\r\n            <div class=\"overview-desc\">Á≠æÂà∞Áéá 81.7%</div>\r\n          </div>\r\n          <div class=\"overview-item\">\r\n            <div class=\"overview-label\">‰ªäÊó•ÂèñÊ∂àÈ¢ÑÁ∫¶</div>\r\n            <div class=\"overview-value\">9</div>\r\n            <div class=\"overview-desc\">Á≥ªÁªüËá™Âä®‰∏éÊâãÂä®ÂêàËÆ°</div>\r\n          </div>\r\n          <div class=\"overview-item\">\r\n            <div class=\"overview-label\">ÂºÇÂ∏∏/ËøùÁ∫¶ÊèêÈÜí</div>\r\n            <div class=\"overview-value warning\">3</div>\r\n            <div class=\"overview-desc\">Âª∫ËÆÆÂèäÊó∂Êü•ÁúãËØ¶ÊÉÖ</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Á¨¨‰∫åË°åÔºöÊúÄËøëÂä®ÊÄÅ + Âø´Êç∑Êìç‰Ωú -->\r\n      <div class=\"dashboard-row\">\r\n        <div class=\"card dashboard-card\">\r\n          <h3 class=\"card-title\">ÊúÄËøëÈ¢ÑÁ∫¶Âä®ÊÄÅ</h3>\r\n          <ul class=\"timeline-list\">\r\n            <li v-for=\"item in latestActivities\" :key=\"item.id\" class=\"timeline-item\">\r\n              <div class=\"timeline-main\">\r\n                <span class=\"timeline-text\">{{ item.text }}</span>\r\n                <span class=\"timeline-tag\" :class=\"'tag-' + item.type\">\r\n                  {{ item.typeLabel }}\r\n                </span>\r\n              </div>\r\n              <div class=\"timeline-time\">{{ item.time }}</div>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n\r\n        <div class=\"card dashboard-card\">\r\n          <h3 class=\"card-title\">Â∏∏Áî®Âø´Êç∑Êìç‰Ωú</h3>\r\n          <div class=\"quick-actions\">\r\n            <el-button type=\"primary\" @click=\"emitChange('admin-reservations')\">\r\n              ÊâìÂºÄÈ¢ÑÁ∫¶ÁÆ°ÁêÜ\r\n            </el-button>\r\n            <el-button @click=\"emitChange('admin-seats')\">\r\n              ÁÆ°ÁêÜËá™‰π†ÂÆ§‰∏éÂ∫ß‰Ωç\r\n            </el-button>\r\n            <el-button @click=\"emitChange('admin-users')\">\r\n              Êü•ÁúãÂ≠¶Áîü‰ø°Áî®ÊÉÖÂÜµ\r\n            </el-button>\r\n            <el-button @click=\"emitChange('admin-logs')\">\r\n              Êü•ÁúãÁ≥ªÁªüÊó•Âøó\r\n            </el-button>\r\n          </div>\r\n          <p class=\"quick-tip\">\r\n            ÊèêÁ§∫Ôºö‰Ω†ÂèØ‰ª•Âú®Â∑¶‰æßÂØºËà™ÈöèÊó∂ÂàáÊç¢‰∏çÂêåÁÆ°ÁêÜÊ®°Âùó„ÄÇ\r\n          </p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- È¢ÑÁ∫¶ÁÆ°ÁêÜÈ°µÈù¢ -->\r\n    <div v-else-if=\"currentPage === 'admin-reservations'\">\r\n      <div class=\"card reservation-card\">\r\n        <!-- È°∂ÈÉ®Ê†áÈ¢ò + Ëá™Âä®Âà∑Êñ∞ÂºÄÂÖ≥ -->\r\n        <div class=\"card-header reservation-header\">\r\n          <div>\r\n            <h2 class=\"page-title\">È¢ÑÁ∫¶ÁÆ°ÁêÜ</h2>\r\n            <p class=\"page-subtitle\">\r\n              Êü•ÁúãÂíåÁÆ°ÁêÜÂ≠¶ÁîüÈ¢ÑÁ∫¶ËÆ∞ÂΩïÔºåÊîØÊåÅÁ≠õÈÄâ„ÄÅÊâπÈáèÊìç‰Ωú„ÄÅË°•ÂΩïÁ≠æÂà∞‰∏éÊ†áËÆ∞ËøùÁ∫¶„ÄÇ\r\n            </p>\r\n          </div>\r\n          <div class=\"reservation-header-right\">\r\n            <div class=\"auto-refresh\">\r\n              <span class=\"auto-refresh-label\">Ëá™Âä®Âà∑Êñ∞</span>\r\n              <el-switch v-model=\"reservationAutoRefresh\" size=\"small\" />\r\n            </div>\r\n            <el-button type=\"primary\" plain size=\"small\" @click=\"handleReservationSearch\">\r\n              Âà∑Êñ∞Êï∞ÊçÆ\r\n            </el-button>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Á≠õÈÄâÂå∫Âüü -->\r\n        <div class=\"filter-bar\">\r\n          <el-input\r\n              v-model=\"reservationFilters.keyword\"\r\n              placeholder=\"ÊåâÂßìÂêç / Â≠¶Âè∑ / È¢ÑÁ∫¶ÁºñÂè∑ÊêúÁ¥¢\"\r\n              clearable\r\n              class=\"filter-item filter-input\"\r\n          >\r\n            <template #prefix>\r\n              <span class=\"input-prefix-icon\">üîç</span>\r\n            </template>\r\n          </el-input>\r\n\r\n          <el-select\r\n              v-model=\"reservationFilters.room\"\r\n              placeholder=\"ÈÄâÊã©Ëá™‰π†ÂÆ§\"\r\n              clearable\r\n              class=\"filter-item filter-select\"\r\n          >\r\n            <el-option\r\n                v-for=\"room in roomOptions\"\r\n                :key=\"room\"\r\n                :label=\"room\"\r\n                :value=\"room\"\r\n            />\r\n          </el-select>\r\n\r\n          <el-date-picker\r\n              v-model=\"reservationFilters.date\"\r\n              type=\"date\"\r\n              placeholder=\"ÈÄâÊã©Êó•Êúü\"\r\n              value-format=\"YYYY-MM-DD\"\r\n              clearable\r\n              class=\"filter-item filter-date\"\r\n          />\r\n\r\n          <el-select\r\n              v-model=\"reservationFilters.status\"\r\n              placeholder=\"È¢ÑÁ∫¶Áä∂ÊÄÅ\"\r\n              clearable\r\n              class=\"filter-item filter-select\"\r\n          >\r\n            <el-option\r\n                v-for=\"item in statusOptions\"\r\n                :key=\"item.value\"\r\n                :label=\"item.label\"\r\n                :value=\"item.value\"\r\n            />\r\n          </el-select>\r\n\r\n          <div class=\"filter-actions\">\r\n            <el-button type=\"primary\" size=\"small\" @click=\"handleReservationSearch\">\r\n              Êü•ËØ¢\r\n            </el-button>\r\n            <el-button size=\"small\" @click=\"resetReservationFilters\">\r\n              ÈáçÁΩÆ\r\n            </el-button>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- ÁªüËÆ°Âå∫Âüü -->\r\n        <div class=\"reservation-stats\">\r\n          <div class=\"stat-card\">\r\n            <div class=\"stat-label\">ÂΩìÂâçËÆ∞ÂΩï</div>\r\n            <div class=\"stat-value\">{{ reservationStats.total }}</div>\r\n          </div>\r\n          <div class=\"stat-card\">\r\n            <div class=\"stat-label\">Â∑≤È¢ÑÁ∫¶</div>\r\n            <div class=\"stat-value\">{{ reservationStats.booked }}</div>\r\n          </div>\r\n          <div class=\"stat-card\">\r\n            <div class=\"stat-label\">Â∑≤Á≠æÂà∞</div>\r\n            <div class=\"stat-value\">{{ reservationStats.signed }}</div>\r\n          </div>\r\n          <div class=\"stat-card\">\r\n            <div class=\"stat-label\">Â∑≤ÂèñÊ∂à / ËøùÁ∫¶</div>\r\n            <div class=\"stat-value\">\r\n              {{ reservationStats.canceled }} / {{ reservationStats.violation }}\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Â∑•ÂÖ∑Ê†èÔºöÊâπÈáèÊìç‰Ωú -->\r\n        <div class=\"reservation-toolbar\">\r\n          <div class=\"toolbar-left\">\r\n            <el-button\r\n                type=\"success\"\r\n                size=\"small\"\r\n                :disabled=\"!reservationSelection.length\"\r\n                @click=\"handleBatchSign\"\r\n            >\r\n              ÊâπÈáèË°•ÂΩïÁ≠æÂà∞\r\n            </el-button>\r\n            <el-button\r\n                type=\"danger\"\r\n                size=\"small\"\r\n                plain\r\n                :disabled=\"!reservationSelection.length\"\r\n                @click=\"handleBatchCancel\"\r\n            >\r\n              ÊâπÈáèÂèñÊ∂àÈ¢ÑÁ∫¶\r\n            </el-button>\r\n            <span v-if=\"reservationSelection.length\" class=\"toolbar-tip\">\r\n              Â∑≤ÈÄâ‰∏≠ {{ reservationSelection.length }} Êù°ËÆ∞ÂΩï\r\n            </span>\r\n          </div>\r\n          <div class=\"toolbar-right\">\r\n            <span class=\"toolbar-tip\">ÂΩìÂâç‰∏∫Á§∫‰æãÊï∞ÊçÆÔºåÂêéÁª≠ÂèØÊõøÊç¢‰∏∫ÂêéÁ´ØÊé•Âè£ËøîÂõûÁöÑÊï∞ÊçÆ„ÄÇ</span>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Êï∞ÊçÆË°®Ê†º -->\r\n        <el-table\r\n            :data=\"paginatedReservations\"\r\n            border\r\n            stripe\r\n            size=\"small\"\r\n            @selection-change=\"handleSelectionChange\"\r\n            class=\"reservation-table\"\r\n        >\r\n          <el-table-column type=\"selection\" width=\"48\" />\r\n          <el-table-column prop=\"code\" label=\"È¢ÑÁ∫¶ÁºñÂè∑\" min-width=\"120\" />\r\n          <el-table-column prop=\"studentName\" label=\"Â≠¶Áîü\" min-width=\"110\">\r\n            <template #default=\"scope\">\r\n              <div class=\"student-cell\">\r\n                <div class=\"student-name\">{{ scope.row.studentName }}</div>\r\n                <div class=\"student-no\">{{ scope.row.studentNo }}</div>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"room\" label=\"Ëá™‰π†ÂÆ§\" min-width=\"130\" />\r\n          <el-table-column prop=\"seatNo\" label=\"Â∫ß‰Ωç\" min-width=\"80\" />\r\n          <el-table-column prop=\"date\" label=\"Êó•Êúü\" min-width=\"110\" />\r\n          <el-table-column prop=\"timeRange\" label=\"Êó∂Èó¥ÊÆµ\" min-width=\"120\" />\r\n          <el-table-column prop=\"status\" label=\"Áä∂ÊÄÅ\" min-width=\"90\">\r\n            <template #default=\"scope\">\r\n              <el-tag :type=\"getStatusTagType(scope.row.status)\" size=\"small\">\r\n                {{ getStatusText(scope.row.status) }}\r\n              </el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column label=\"Â§áÊ≥®\" min-width=\"160\">\r\n            <template #default=\"scope\">\r\n              <span class=\"remark-text\">\r\n                {{ scope.row.remark || '‚Äî' }}\r\n              </span>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column label=\"Êìç‰Ωú\" fixed=\"right\" min-width=\"200\">\r\n            <template #default=\"scope\">\r\n              <div class=\"action-buttons\">\r\n                <el-button\r\n                    v-if=\"scope.row.status === 'booked'\"\r\n                    type=\"primary\"\r\n                    link\r\n                    size=\"small\"\r\n                    @click=\"handleSign(scope.row)\"\r\n                >\r\n                  Ë°•ÂΩïÁ≠æÂà∞\r\n                </el-button>\r\n                <el-button\r\n                    v-if=\"scope.row.status === 'booked'\"\r\n                    type=\"danger\"\r\n                    link\r\n                    size=\"small\"\r\n                    @click=\"handleCancel(scope.row)\"\r\n                >\r\n                  ÂèñÊ∂àÈ¢ÑÁ∫¶\r\n                </el-button>\r\n\r\n                <el-dropdown v-if=\"scope.row.status !== 'canceled'\">\r\n                  <span class=\"el-dropdown-link\">Êõ¥Â§ö</span>\r\n                  <template #dropdown>\r\n                    <el-dropdown-menu>\r\n                      <el-dropdown-item @click=\"handleMarkViolation(scope.row)\">\r\n                        Ê†áËÆ∞ËøùÁ∫¶\r\n                      </el-dropdown-item>\r\n                      <el-dropdown-item @click=\"handleViewDetail(scope.row)\">\r\n                        Êü•ÁúãËØ¶ÊÉÖ\r\n                      </el-dropdown-item>\r\n                    </el-dropdown-menu>\r\n                  </template>\r\n                </el-dropdown>\r\n\r\n                <span v-else class=\"disabled-text\">‰∏çÂèØÊìç‰Ωú</span>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n\r\n        <!-- ÂàÜÈ°µ -->\r\n        <div class=\"table-footer\">\r\n          <el-pagination\r\n              background\r\n              layout=\"total, prev, pager, next, sizes\"\r\n              :total=\"filteredReservations.length\"\r\n              :page-sizes=\"[5, 10, 20]\"\r\n              :page-size=\"reservationPageSize\"\r\n              :current-page=\"reservationPage\"\r\n              @size-change=\"handlePageSizeChange\"\r\n              @current-change=\"handlePageChange\"\r\n          />\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Áî®Êà∑ÁÆ°ÁêÜÔºàÂçïÁã¨ÁªÑ‰ª∂Ôºâ -->\r\n    <AdminUserManage v-else-if=\"currentPage === 'admin-users'\" />\r\n\r\n    <!-- Â∫ß‰ΩçÁÆ°ÁêÜ -->\r\n    <div v-else-if=\"currentPage === 'admin-seats'\" class=\"card simple-card\">\r\n      <h2 class=\"page-title\">Â∫ß‰ΩçÁÆ°ÁêÜ</h2>\r\n      <p class=\"page-subtitle\">ÈÖçÁΩÆËá™‰π†ÂÆ§ÂºÄÊîæÊó∂ÊÆµ„ÄÅÂ∫ß‰ΩçÂ∏ÉÂ±Ä‰∏éÁ¶ÅÁî®Âå∫ÂüüÔºàÊ≠§Â§Ñ‰∏∫Á§∫‰æãÂç†‰ΩçÔºâ„ÄÇ</p>\r\n      <p class=\"simple-tip\">\r\n        ÂèØ‰ª•ÁªìÂêàÊïôÂÆ§/Ê•ºÊ†ã„ÄÅÊó∂Èó¥ÊÆµÁ≠â‰ø°ÊÅØÔºåÂÅöÊõ¥Á≤æÁªÜÁöÑÂ∫ß‰ΩçÈÖçÁΩÆ‰∏éÊâπÈáèÂØºÂÖ•ÂØºÂá∫„ÄÇ\r\n      </p>\r\n    </div>\r\n\r\n    <!-- ÊäïËØâÂ§ÑÁêÜ -->\r\n    <div v-else-if=\"currentPage === 'admin-complaints'\" class=\"card simple-card\">\r\n      <h2 class=\"page-title\">ÊäïËØâÂ§ÑÁêÜ</h2>\r\n      <p class=\"page-subtitle\">Â§ÑÁêÜÂ≠¶ÁîüÂèçÈ¶à‰∏éÊäïËØâËÆ∞ÂΩïÔºåË∑üË∏™Â§ÑÁêÜÁªìÊûú‰∏éÂèçÈ¶àÊª°ÊÑèÂ∫¶„ÄÇ</p>\r\n      <p class=\"simple-tip\">\r\n        ÂêéÁª≠ÂèØÂú®Ê≠§È°µÈù¢Â¢ûÂä†ÊäïËØâÂàóË°®„ÄÅÂ§ÑÁêÜËøõÂ∫¶„ÄÅÊ†áËÆ∞Â∑≤ËØª„ÄÅÂØºÂá∫Á≠âÂäüËÉΩ„ÄÇ\r\n      </p>\r\n    </div>\r\n\r\n    <!-- Êó•ÂøóÁªüËÆ° -->\r\n    <div v-else-if=\"currentPage === 'admin-logs'\" class=\"card simple-card\">\r\n      <h2 class=\"page-title\">Êó•ÂøóÁªüËÆ°</h2>\r\n      <p class=\"page-subtitle\">Êü•ÁúãÁ≥ªÁªüÊìç‰ΩúÊó•Âøó‰∏éÁªüËÆ°‰ø°ÊÅØÔºåËæÖÂä©ÊéíÊü•ÈóÆÈ¢ò‰∏éËøΩË∏™Êìç‰ΩúËÆ∞ÂΩï„ÄÇ</p>\r\n      <p class=\"simple-tip\">\r\n        ÂèØ‰ª•Âú®Ê≠§Êé•ÂÖ•ÊäòÁ∫øÂõæ/Êü±Áä∂ÂõæÂ±ïÁ§∫Êó•È¢ÑÁ∫¶Èáè„ÄÅÁ≠æÂà∞Áéá„ÄÅËøùÁ∫¶ÁéáÁ≠âÁªüËÆ°Êï∞ÊçÆ„ÄÇ\r\n      </p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AdminUserManage from './AdminUserManage.vue'\r\n\r\nexport default {\r\n  name: 'AdminHome',\r\n  components: {\r\n    AdminUserManage\r\n  },\r\n  props: {\r\n    currentPage: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  emits: ['change-page'],\r\n  data () {\r\n    return {\r\n      // ‰ª™Ë°®ÁõòÁ§∫‰æãÊï∞ÊçÆ\r\n      latestActivities: [\r\n        {\r\n          id: 1,\r\n          text: 'Âº†‰∏â È¢ÑÁ∫¶‰∫Ü Âõæ‰π¶È¶Ü 401 ¬∑ 10:00-12:00',\r\n          type: 'book',\r\n          typeLabel: 'Êñ∞È¢ÑÁ∫¶',\r\n          time: '09:15'\r\n        },\r\n        {\r\n          id: 2,\r\n          text: 'ÊùéÂõõ Âú® Êú¨ÈÉ® ¬∑ 3 Ê•º 301 ÂÆåÊàêÁ≠æÂà∞',\r\n          type: 'sign',\r\n          typeLabel: 'Â∑≤Á≠æÂà∞',\r\n          time: '08:05'\r\n        },\r\n        {\r\n          id: 3,\r\n          text: 'Áéã‰∫î ÂèñÊ∂à‰∫Ü ÊòéÊó•ÁöÑÈ¢ÑÁ∫¶ÔºàÂéüÂõ†ÔºöËØæÁ®ãË∞ÉÊï¥Ôºâ',\r\n          type: 'cancel',\r\n          typeLabel: 'Â∑≤ÂèñÊ∂à',\r\n          time: '07:48'\r\n        }\r\n      ],\r\n\r\n      /* ========= È¢ÑÁ∫¶ÁÆ°ÁêÜÁõ∏ÂÖ≥ ========= */\r\n      reservationFilters: {\r\n        keyword: '',\r\n        room: '',\r\n        date: '',\r\n        status: ''\r\n      },\r\n      reservationAutoRefresh: false,\r\n      roomOptions: ['3 Ê•º 301', 'Âõæ‰π¶È¶Ü 401', 'Âõæ‰π¶È¶Ü 501'],\r\n      statusOptions: [\r\n        { label: 'Â∑≤È¢ÑÁ∫¶', value: 'booked' },\r\n        { label: 'Â∑≤Á≠æÂà∞', value: 'signed' },\r\n        { label: 'Â∑≤ÂèñÊ∂à', value: 'canceled' },\r\n        { label: 'ËøùÁ∫¶', value: 'violation' }\r\n      ],\r\n      reservations: [\r\n        {\r\n          id: 1,\r\n          code: 'Y20251204001',\r\n          studentNo: '20210001',\r\n          studentName: 'Âº†‰∏â',\r\n          room: '3 Ê•º 301',\r\n          seatNo: 'A-15',\r\n          date: '2025-12-05',\r\n          timeRange: '08:00-10:00',\r\n          status: 'booked',\r\n          remark: 'ËøüÂà∞ÈúÄ‰∫∫Â∑•Á°ÆËÆ§'\r\n        },\r\n        {\r\n          id: 2,\r\n          code: 'Y20251204002',\r\n          studentNo: '20210002',\r\n          studentName: 'ÊùéÂõõ',\r\n          room: 'Âõæ‰π¶È¶Ü 401',\r\n          seatNo: 'B-08',\r\n          date: '2025-12-05',\r\n          timeRange: '10:00-12:00',\r\n          status: 'signed',\r\n          remark: 'ÊåâÊó∂Á≠æÂà∞'\r\n        },\r\n        {\r\n          id: 3,\r\n          code: 'Y20251204003',\r\n          studentNo: '20210003',\r\n          studentName: 'Áéã‰∫î',\r\n          room: 'Âõæ‰π¶È¶Ü 401',\r\n          seatNo: 'C-07',\r\n          date: '2025-12-05',\r\n          timeRange: '14:00-16:00',\r\n          status: 'canceled',\r\n          remark: 'Â≠¶Áîü‰∏ªÂä®ÂèñÊ∂à'\r\n        },\r\n        {\r\n          id: 4,\r\n          code: 'Y20251204004',\r\n          studentNo: '20210004',\r\n          studentName: 'ËµµÂÖ≠',\r\n          room: '3 Ê•º 301',\r\n          seatNo: 'A-01',\r\n          date: '2025-12-04',\r\n          timeRange: '18:00-20:00',\r\n          status: 'violation',\r\n          remark: 'Êú™Âà∞Âú∫‰∏îÊú™ÂèñÊ∂à'\r\n        }\r\n      ],\r\n      reservationSelection: [],\r\n      reservationPage: 1,\r\n      reservationPageSize: 10\r\n    }\r\n  },\r\n  computed: {\r\n    todayText () {\r\n      const d = new Date()\r\n      const y = d.getFullYear()\r\n      const m = String(d.getMonth() + 1).padStart(2, '0')\r\n      const day = String(d.getDate()).padStart(2, '0')\r\n      return `${y}-${m}-${day}`\r\n    },\r\n    filteredReservations () {\r\n      const { keyword, room, date, status } = this.reservationFilters\r\n      return this.reservations.filter(item => {\r\n        let ok = true\r\n        if (keyword && keyword.trim()) {\r\n          const k = keyword.trim()\r\n          ok =\r\n              ok &&\r\n              (item.studentName.includes(k) ||\r\n                  item.studentNo.includes(k) ||\r\n                  item.code.includes(k))\r\n        }\r\n        if (room) {\r\n          ok = ok && item.room === room\r\n        }\r\n        if (date) {\r\n          ok = ok && item.date === date\r\n        }\r\n        if (status) {\r\n          ok = ok && item.status === status\r\n        }\r\n        return ok\r\n      })\r\n    },\r\n    paginatedReservations () {\r\n      const start = (this.reservationPage - 1) * this.reservationPageSize\r\n      return this.filteredReservations.slice(\r\n          start,\r\n          start + this.reservationPageSize\r\n      )\r\n    },\r\n    reservationStats () {\r\n      const stats = {\r\n        total: 0,\r\n        booked: 0,\r\n        signed: 0,\r\n        canceled: 0,\r\n        violation: 0\r\n      }\r\n      stats.total = this.filteredReservations.length\r\n      this.filteredReservations.forEach(item => {\r\n        if (item.status === 'booked') stats.booked += 1\r\n        else if (item.status === 'signed') stats.signed += 1\r\n        else if (item.status === 'canceled') stats.canceled += 1\r\n        else if (item.status === 'violation') stats.violation += 1\r\n      })\r\n      return stats\r\n    }\r\n  },\r\n  methods: {\r\n    emitChange (page) {\r\n      this.$emit('change-page', page)\r\n    },\r\n    /* ===== È¢ÑÁ∫¶ÁÆ°ÁêÜÊñπÊ≥ï ===== */\r\n    handleReservationSearch () {\r\n      console.log('ÊâßË°åÈ¢ÑÁ∫¶Êü•ËØ¢', this.reservationFilters)\r\n      this.reservationPage = 1\r\n    },\r\n    resetReservationFilters () {\r\n      this.reservationFilters = {\r\n        keyword: '',\r\n        room: '',\r\n        date: '',\r\n        status: ''\r\n      }\r\n      this.reservationPage = 1\r\n    },\r\n    handleSelectionChange (val) {\r\n      this.reservationSelection = val || []\r\n    },\r\n    handlePageChange (page) {\r\n      this.r