{"ast":null,"code":"import { ElMessageBox, ElMessage } from 'element-plus';\nimport AdminHeader from './AdminHeader.vue';\nimport AdminAside from './AdminAside.vue';\nimport AdminHome from './AdminHome.vue';\nexport default {\n  name: 'AdminIndex',\n  components: {\n    AdminHeader,\n    AdminAside,\n    AdminHome\n  },\n  data() {\n    return {\n      currentPage: 'admin-home'\n    };\n  },\n  methods: {\n    changePage(page) {\n      this.currentPage = page;\n    },\n    async handleLogout() {\n      try {\n        await ElMessageBox.confirm('确定要退出登录吗？', '提示', {\n          confirmButtonText: '退出',\n          cancelButtonText: '取消',\n          type: 'warning'\n        });\n\n        // 关键：路由守卫用的是 ssrmsUser，所以必须删它\n        localStorage.removeItem('ssrmsUser');\n\n        // 如果你登录时还存过 token，也顺手清掉（不影响没存的情况）\n        localStorage.removeItem('ssrmsToken');\n        localStorage.removeItem('token');\n        sessionStorage.removeItem('ssrmsUser');\n        sessionStorage.removeItem('ssrmsToken');\n        sessionStorage.removeItem('token');\n\n        // 可选：如果你给 axios 设过默认 Authorization，这里顺手清\n        if (this.$axios && this.$axios.defaults && this.$axios.defaults.headers) {\n          delete this.$axios.defaults.headers.common.Authorization;\n        }\n        ElMessage.success('已退出登录');\n        this.$router.replace('/login');\n      } catch (e) {\n        // 点了取消就不做任何事\n      }\n    }\n  }\n};","map":{"version":3,"names":["ElMessageBox","ElMessage","AdminHeader","AdminAside","AdminHome","name","components","data","currentPage","methods","changePage","page","handleLogout","confirm","confirmButtonText","cancelButtonText","type","localStorage","removeItem","sessionStorage","$axios","defaults","headers","common","Authorization","success","$router","replace","e"],"sources":["D:\\code\\SpringBoot-Vue-SSRMS\\ssrms-web\\src\\components\\AdminIndex.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-wrapper\">\r\n    <AdminAside\r\n        :current-page=\"currentPage\"\r\n        @change-page=\"changePage\"\r\n    />\r\n\r\n    <div class=\"main-area\">\r\n      <AdminHeader\r\n          :current-page=\"currentPage\"\r\n          @logout=\"handleLogout\"\r\n      />\r\n      <div class=\"content-area\">\r\n        <AdminHome\r\n            :current-page=\"currentPage\"\r\n            @change-page=\"changePage\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ElMessageBox, ElMessage } from 'element-plus'\r\nimport AdminHeader from './AdminHeader.vue'\r\nimport AdminAside from './AdminAside.vue'\r\nimport AdminHome from './AdminHome.vue'\r\n\r\nexport default {\r\n  name: 'AdminIndex',\r\n  components: {\r\n    AdminHeader,\r\n    AdminAside,\r\n    AdminHome\r\n  },\r\n  data () {\r\n    return {\r\n      currentPage: 'admin-home'\r\n    }\r\n  },\r\n  methods: {\r\n    changePage (page) {\r\n      this.currentPage = page\r\n    },\r\n\r\n    async handleLogout () {\r\n      try {\r\n        await ElMessageBox.confirm(\r\n            '确定要退出登录吗？',\r\n            '提示',\r\n            {\r\n              confirmButtonText: '退出',\r\n              cancelButtonText: '取消',\r\n              type: 'warning'\r\n            }\r\n        )\r\n\r\n        // 关键：路由守卫用的是 ssrmsUser，所以必须删它\r\n        localStorage.removeItem('ssrmsUser')\r\n\r\n        // 如果你登录时还存过 token，也顺手清掉（不影响没存的情况）\r\n        localStorage.removeItem('ssrmsToken')\r\n        localStorage.removeItem('token')\r\n        sessionStorage.removeItem('ssrmsUser')\r\n        sessionStorage.removeItem('ssrmsToken')\r\n        sessionStorage.removeItem('token')\r\n\r\n        // 可选：如果你给 axios 设过默认 Authorization，这里顺手清\r\n        if (this.$axios && this.$axios.defaults && this.$axios.defaults.headers) {\r\n          delete this.$axios.defaults.headers.common.Authorization\r\n        }\r\n\r\n        ElMessage.success('已退出登录')\r\n        this.$router.replace('/login')\r\n      } catch (e) {\r\n        // 点了取消就不做任何事\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 整体布局与 UserIndex 保持一致 */\r\n.app-wrapper {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  background-color: #f5f7fb; /* 统一背景色 */\r\n}\r\n\r\n.main-area {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.content-area {\r\n  flex: 1;\r\n  display: flex;\r\n  padding: 8px 24px 20px 24px;\r\n  box-sizing: border-box;\r\n  overflow-x: hidden;\r\n}\r\n\r\n@media (max-width: 900px) {\r\n  .app-wrapper {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .content-area {\r\n    padding: 8px 12px 16px 12px;\r\n  }\r\n}\r\n</style>"],"mappings":"AAuBA,SAASA,YAAY,EAAEC,SAAQ,QAAS,cAAa;AACrD,OAAOC,WAAU,MAAO,mBAAkB;AAC1C,OAAOC,UAAS,MAAO,kBAAiB;AACxC,OAAOC,SAAQ,MAAO,iBAAgB;AAEtC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVJ,WAAW;IACXC,UAAU;IACVC;EACF,CAAC;EACDG,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAASA,CAAGC,IAAI,EAAE;MAChB,IAAI,CAACH,WAAU,GAAIG,IAAG;IACxB,CAAC;IAED,MAAMC,YAAWA,CAAA,EAAK;MACpB,IAAI;QACF,MAAMZ,YAAY,CAACa,OAAO,CACtB,WAAW,EACX,IAAI,EACJ;UACEC,iBAAiB,EAAE,IAAI;UACvBC,gBAAgB,EAAE,IAAI;UACtBC,IAAI,EAAE;QACR,CACJ;;QAEA;QACAC,YAAY,CAACC,UAAU,CAAC,WAAW;;QAEnC;QACAD,YAAY,CAACC,UAAU,CAAC,YAAY;QACpCD,YAAY,CAACC,UAAU,CAAC,OAAO;QAC/BC,cAAc,CAACD,UAAU,CAAC,WAAW;QACrCC,cAAc,CAACD,UAAU,CAAC,YAAY;QACtCC,cAAc,CAACD,UAAU,CAAC,OAAO;;QAEjC;QACA,IAAI,IAAI,CAACE,MAAK,IAAK,IAAI,CAACA,MAAM,CAACC,QAAO,IAAK,IAAI,CAACD,MAAM,CAACC,QAAQ,CAACC,OAAO,EAAE;UACvE,OAAO,IAAI,CAACF,MAAM,CAACC,QAAQ,CAACC,OAAO,CAACC,MAAM,CAACC,aAAY;QACzD;QAEAvB,SAAS,CAACwB,OAAO,CAAC,OAAO;QACzB,IAAI,CAACC,OAAO,CAACC,OAAO,CAAC,QAAQ;MAC/B,EAAE,OAAOC,CAAC,EAAE;QACV;MAAA;IAEJ;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}