{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nexport default {\n  name: 'AdminHome',\n  props: {\n    currentPage: {\n      type: String,\n      required: true\n    }\n  },\n  emits: ['change-page'],\n  data() {\n    return {\n      // ä»ªè¡¨ç›˜ç¤ºä¾‹æ•°æ®\n      latestActivities: [{\n        id: 1,\n        text: 'å¼ ä¸‰ é¢„çº¦äº† å›¾ä¹¦é¦† 401 Â· 10:00-12:00',\n        type: 'book',\n        typeLabel: 'æ–°é¢„çº¦',\n        time: '09:15'\n      }, {\n        id: 2,\n        text: 'æå›› åœ¨ æœ¬éƒ¨ Â· 3 æ¥¼ 301 å®Œæˆç­¾åˆ°',\n        type: 'sign',\n        typeLabel: 'å·²ç­¾åˆ°',\n        time: '08:05'\n      }, {\n        id: 3,\n        text: 'ç‹äº” å–æ¶ˆäº† æ˜æ—¥çš„é¢„çº¦ï¼ˆåŸå› ï¼šè¯¾ç¨‹è°ƒæ•´ï¼‰',\n        type: 'cancel',\n        typeLabel: 'å·²å–æ¶ˆ',\n        time: '07:48'\n      }],\n      // é¢„çº¦ç®¡ç†ï¼šç­›é€‰æ¡ä»¶\n      reservationFilters: {\n        keyword: '',\n        room: '',\n        date: '',\n        status: ''\n      },\n      reservationAutoRefresh: false,\n      roomOptions: ['3 æ¥¼ 301', 'å›¾ä¹¦é¦† 401', 'å›¾ä¹¦é¦† 501'],\n      statusOptions: [{\n        label: 'å·²é¢„çº¦',\n        value: 'booked'\n      }, {\n        label: 'å·²ç­¾åˆ°',\n        value: 'signed'\n      }, {\n        label: 'å·²å–æ¶ˆ',\n        value: 'canceled'\n      }, {\n        label: 'è¿çº¦',\n        value: 'violation'\n      }],\n      // é¢„çº¦æ•°æ®ç¤ºä¾‹ï¼ˆåç»­å¯æ›¿æ¢ä¸ºåç«¯æ¥å£è¿”å›ï¼‰\n      reservations: [{\n        id: 1,\n        code: 'Y20251204001',\n        studentNo: '20210001',\n        studentName: 'å¼ ä¸‰',\n        room: '3 æ¥¼ 301',\n        seatNo: 'A-15',\n        date: '2025-12-05',\n        timeRange: '08:00-10:00',\n        status: 'booked',\n        remark: 'è¿Ÿåˆ°éœ€äººå·¥ç¡®è®¤'\n      }, {\n        id: 2,\n        code: 'Y20251204002',\n        studentNo: '20210002',\n        studentName: 'æå››',\n        room: 'å›¾ä¹¦é¦† 401',\n        seatNo: 'B-08',\n        date: '2025-12-05',\n        timeRange: '10:00-12:00',\n        status: 'signed',\n        remark: 'æŒ‰æ—¶ç­¾åˆ°'\n      }, {\n        id: 3,\n        code: 'Y20251204003',\n        studentNo: '20210003',\n        studentName: 'ç‹äº”',\n        room: 'å›¾ä¹¦é¦† 401',\n        seatNo: 'C-07',\n        date: '2025-12-05',\n        timeRange: '14:00-16:00',\n        status: 'canceled',\n        remark: 'å­¦ç”Ÿä¸»åŠ¨å–æ¶ˆ'\n      }, {\n        id: 4,\n        code: 'Y20251204004',\n        studentNo: '20210004',\n        studentName: 'èµµå…­',\n        room: '3 æ¥¼ 301',\n        seatNo: 'A-01',\n        date: '2025-12-04',\n        timeRange: '18:00-20:00',\n        status: 'violation',\n        remark: 'æœªåˆ°åœºä¸”æœªå–æ¶ˆ'\n      }],\n      reservationSelection: [],\n      reservationPage: 1,\n      reservationPageSize: 10\n    };\n  },\n  computed: {\n    todayText() {\n      const d = new Date();\n      const y = d.getFullYear();\n      const m = String(d.getMonth() + 1).padStart(2, '0');\n      const day = String(d.getDate()).padStart(2, '0');\n      return `${y}-${m}-${day}`;\n    },\n    filteredReservations() {\n      const {\n        keyword,\n        room,\n        date,\n        status\n      } = this.reservationFilters;\n      return this.reservations.filter(item => {\n        let ok = true;\n        if (keyword && keyword.trim()) {\n          const k = keyword.trim();\n          ok = ok && (item.studentName.includes(k) || item.studentNo.includes(k) || item.code.includes(k));\n        }\n        if (room) {\n          ok = ok && item.room === room;\n        }\n        if (date) {\n          ok = ok && item.date === date;\n        }\n        if (status) {\n          ok = ok && item.status === status;\n        }\n        return ok;\n      });\n    },\n    paginatedReservations() {\n      const start = (this.reservationPage - 1) * this.reservationPageSize;\n      return this.filteredReservations.slice(start, start + this.reservationPageSize);\n    },\n    reservationStats() {\n      const stats = {\n        total: 0,\n        booked: 0,\n        signed: 0,\n        canceled: 0,\n        violation: 0\n      };\n      stats.total = this.filteredReservations.length;\n      this.filteredReservations.forEach(item => {\n        if (item.status === 'booked') stats.booked += 1;else if (item.status === 'signed') stats.signed += 1;else if (item.status === 'canceled') stats.canceled += 1;else if (item.status === 'violation') stats.violation += 1;\n      });\n      return stats;\n    }\n  },\n  methods: {\n    emitChange(page) {\n      this.$emit('change-page', page);\n    },\n    // æŸ¥è¯¢æŒ‰é’®ï¼ˆç›®å‰åªåœ¨å‰ç«¯è¿‡æ»¤ï¼Œåç»­å¯æ¥åç«¯æ¥å£ï¼‰\n    handleReservationSearch() {\n      // è¿™é‡Œé¢„ç•™ç»™æ¥å£è°ƒç”¨\n      console.log('æ‰§è¡Œé¢„çº¦æŸ¥è¯¢', this.reservationFilters);\n      // æŸ¥è¯¢æ—¶é‡ç½®åˆ°ç¬¬ä¸€é¡µ\n      this.reservationPage = 1;\n    },\n    resetReservationFilters() {\n      this.reservationFilters = {\n        keyword: '',\n        room: '',\n        date: '',\n        status: ''\n      };\n      this.reservationPage = 1;\n    },\n    handleSelectionChange(val) {\n      this.reservationSelection = val || [];\n    },\n    handlePageChange(page) {\n      this.reservationPage = page;\n    },\n    handlePageSizeChange(size) {\n      this.reservationPageSize = size;\n      this.reservationPage = 1;\n    },\n    getStatusTagType(status) {\n      if (status === 'booked') return 'info';\n      if (status === 'signed') return 'success';\n      if (status === 'canceled') return 'warning';\n      if (status === 'violation') return 'danger';\n      return '';\n    },\n    getStatusText(status) {\n      if (status === 'booked') return 'å·²é¢„çº¦';\n      if (status === 'signed') return 'å·²ç­¾åˆ°';\n      if (status === 'canceled') return 'å·²å–æ¶ˆ';\n      if (status === 'violation') return 'è¿çº¦';\n      return status;\n    },\n    handleSign(row) {\n      console.log('è¡¥å½•ç­¾åˆ°', row);\n      // è¿™é‡Œåªæ˜¯å‰ç«¯ç¤ºä¾‹ï¼Œå¯ä»¥åœ¨æˆåŠŸåæ‰‹åŠ¨æ›´æ–°è¡ŒçŠ¶æ€\n      row.status = 'signed';\n    },\n    handleCancel(row) {\n      console.log('å–æ¶ˆé¢„çº¦', row);\n      row.status = 'canceled';\n    },\n    handleMarkViolation(row) {\n      console.log('æ ‡è®°è¿çº¦', row);\n      row.status = 'violation';\n    },\n    handleViewDetail(row) {\n      console.log('æŸ¥çœ‹é¢„çº¦è¯¦æƒ…', row);\n      // å¯åç»­æ›¿æ¢ä¸ºå¼¹çª—æˆ–ä¾§è¾¹æŠ½å±‰å±•ç¤ºè¯¦ç»†ä¿¡æ¯\n    },\n    handleBatchSign() {\n      console.log('æ‰¹é‡è¡¥å½•ç­¾åˆ°', this.reservationSelection);\n      this.reservationSelection.forEach(item => {\n        if (item.status === 'booked') item.status = 'signed';\n      });\n      this.reservationSelection = [];\n    },\n    handleBatchCancel() {\n      console.log('æ‰¹é‡å–æ¶ˆé¢„çº¦', this.reservationSelection);\n      this.reservationSelection.forEach(item => {\n        if (item.status === 'booked') item.status = 'canceled';\n      });\n      this.reservationSelection = [];\n    }\n  }\n};","map":{"version":3,"names":["name","props","currentPage","type","String","required","emits","data","latestActivities","id","text","typeLabel","time","reservationFilters","keyword","room","date","status","reservationAutoRefresh","roomOptions","statusOptions","label","value","reservations","code","studentNo","studentName","seatNo","timeRange","remark","reservationSelection","reservationPage","reservationPageSize","computed","todayText","d","Date","y","getFullYear","m","getMonth","padStart","day","getDate","filteredReservations","filter","item","ok","trim","k","includes","paginatedReservations","start","slice","reservationStats","stats","total","booked","signed","canceled","violation","length","forEach","methods","emitChange","page","$emit","handleReservationSearch","console","log","resetReservationFilters","handleSelectionChange","val","handlePageChange","handlePageSizeChange","size","getStatusTagType","getStatusText","handleSign","row","handleCancel","handleMarkViolation","handleViewDetail","handleBatchSign","handleBatchCancel"],"sources":["D:\\code\\SpringBoot-Vue-SSRMS\\ssrms-web\\src\\components\\AdminHome.vue"],"sourcesContent":["<template>\r\n  <div class=\"main\">\r\n    <!-- ç®¡ç†å‘˜é¦–é¡µä»ªè¡¨ç›˜ -->\r\n    <div v-if=\"currentPage === 'admin-home'\" class=\"dashboard-wrapper\">\r\n      <!-- ä»Šæ—¥æ¦‚è§ˆ -->\r\n      <div class=\"card overview-card\">\r\n        <div class=\"overview-header\">\r\n          <div>\r\n            <h2 class=\"section-title\">ä»Šæ—¥æ¦‚è§ˆ</h2>\r\n            <p class=\"section-subtitle\">å¿«é€Ÿäº†è§£ä»Šæ—¥é¢„çº¦ã€ç­¾åˆ°ä¸å¼‚å¸¸æƒ…å†µã€‚</p>\r\n          </div>\r\n          <div class=\"overview-extra\">\r\n            <span class=\"overview-date\">ä»Šå¤©ï¼š{{ todayText }}</span>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"overview-grid\">\r\n          <div class=\"overview-item\">\r\n            <div class=\"overview-label\">ä»Šæ—¥é¢„çº¦æ€»æ•°</div>\r\n            <div class=\"overview-value\">120</div>\r\n            <div class=\"overview-desc\">ç›¸æ¯”æ˜¨æ—¥ â†‘ 12%</div>\r\n          </div>\r\n          <div class=\"overview-item\">\r\n            <div class=\"overview-label\">ä»Šæ—¥å·²ç­¾åˆ°</div>\r\n            <div class=\"overview-value\">98</div>\r\n            <div class=\"overview-desc\">ç­¾åˆ°ç‡ 81.7%</div>\r\n          </div>\r\n          <div class=\"overview-item\">\r\n            <div class=\"overview-label\">ä»Šæ—¥å–æ¶ˆé¢„çº¦</div>\r\n            <div class=\"overview-value\">9</div>\r\n            <div class=\"overview-desc\">ç³»ç»Ÿè‡ªåŠ¨ä¸æ‰‹åŠ¨åˆè®¡</div>\r\n          </div>\r\n          <div class=\"overview-item\">\r\n            <div class=\"overview-label\">å¼‚å¸¸/è¿çº¦æé†’</div>\r\n            <div class=\"overview-value warning\">3</div>\r\n            <div class=\"overview-desc\">å»ºè®®åŠæ—¶æŸ¥çœ‹è¯¦æƒ…</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- ç¬¬äºŒè¡Œï¼šæœ€è¿‘åŠ¨æ€ + å¿«æ·æ“ä½œ -->\r\n      